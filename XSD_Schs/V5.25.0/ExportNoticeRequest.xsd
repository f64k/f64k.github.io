	<xs:simpleType name="AccDocNoType">
		<xs:annotation>
			<xs:documentation>Номер платежного документа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="\d{1,6}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountNumType">
		<xs:annotation>
			<xs:documentation>Номер счета</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:length value="20"/>
			<xs:pattern value="\d{20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ClaimerTypeType">
		<xs:annotation>
			<xs:documentation>Формат типа заявителя</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:IntegerCategoriesCust">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>юридическое лицо</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>физическое лицо</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>индивидуальный предприниматель</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1700">
				<xs:annotation>
					<xs:documentation>орган, уполномоченный от имени Российской Федерации, субъекта Российской Федерации или муниципального образования осуществлять права и исполнять обязанности в исполните</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ClarificationIdType">
		<xs:annotation>
			<xs:documentation>Уникальный идентификатор уточнения вида и принадлежности платежа (УВПП)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="5\d{4}((0[1-9]|[12][0-9]|3[01])(0[1-9]|1[012])\d{4})\d{19}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Code7Type">
		<xs:annotation>
			<xs:documentation>Строка 7 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:CodeCategoriesCust">
			<xs:length value="7"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DiscountDateType">
		<xs:annotation>
			<xs:documentation>Срок действия скидки.
                Возможные значения:
                - дата окончания скидки в формате «ГГГГ-ММ-ДД»;
                - при отсутствии срока действия указывается значение «0».</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="(\d{4}-\d{2}-\d{2})|0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IncomeIdType">
		<xs:annotation>
			<xs:documentation>Уникальный идентификатор зачисления (УИЗ)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="4\d{4}((0[1-9]|[12][0-9]|3[01])(0[1-9]|1[012])\d{4})\d{19}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="INNType">
		<xs:annotation>
			<xs:documentation>ИНН юр. лица</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:CodeCategoriesCust">
			<xs:pattern value="([^0^\D]\d|\d[^0^\D])\d{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KPPType">
		<xs:annotation>
			<xs:documentation>КПП юр. лица</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:CodeCategoriesCust">
			<xs:pattern value="([^0^\D]\d|\d[^0^\D])\d{2}[A-Z0-9]{2}\d{3}">
				<xs:annotation>
					<xs:documentation>9 символов, пятый и шестой из которых могут быть прописными (заглавными) латинскими буквами или цифрами, а все остальные только цифрами, и при этом первый и второй знаки (цифры) не могут одновременно принимать значение ноль ("0").</xs:documentation>
				</xs:annotation>
			</xs:pattern>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kodUBPType">
		<xs:annotation>
			<xs:documentation>Формат уникального кода плательщика - прямого участника системы казначейских платежей в соответствии со Сводным реестром</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="\w{5}"/>
			<xs:pattern value="\w{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LongType">
		<xs:annotation>
			<xs:documentation>Целое число от - 92233720368547758087 до 9223372036854775807</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:minInclusive value="-9223372036854775808"/>
			<xs:maxInclusive value="9223372036854775807"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MeaningType">
		<xs:annotation>
			<xs:documentation>Статус, отражающий изменение данных</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:CodeCategoriesCust">
			<xs:length value="1"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NonNegativeIntegerValueType">
		<xs:annotation>
			<xs:documentation>Целое неотрицательное число от 0 до 18446744073709551615</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:IntegerCategoriesCust">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="18446744073709551615"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OrgNameType">
		<xs:annotation>
			<xs:documentation>Строка длиной до 2000 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:maxLength value="2000"/>
			<xs:pattern value="[\S\t ]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OtherType">
		<xs:annotation>
			<xs:documentation>Прием в кассу наличных денежных средств</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:enumeration value="CASH">
				<xs:annotation>
					<xs:documentation>Прием в кассу наличных денежных средств </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PayeeAccountType">
		<xs:annotation>
			<xs:documentation>Лицевого счета получателя платежа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="[\d\w]{11}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PayerIdentifierType">
		<xs:annotation>
			<xs:documentation>Идентификатор плательщика</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:IdentifierCategoriesCust">
			<xs:pattern value="1((0[1-9])|(1[0-5])|(2[12456789])|(3[0]))[0-9a-zA-Zа-яА-Я]{19}">
				<xs:annotation>
					<xs:documentation>Идентификатор ФЛ</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="200\d{14}[A-Z0-9]{2}\d{3}">
				<xs:annotation>
					<xs:documentation>Идентификатор ЮЛ или ИП</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="300[0-9a-zA-Zа-яА-Я]{19}"/>
			<xs:pattern value="(2|3)00\d{10}[0]{9}"/>
			<xs:pattern value="4[0]{9}\d{12}"/>
			<xs:pattern value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AdditionalPayerIdentifierType">
		<xs:annotation>
			<xs:documentation>Идентификатор плательщика дополнительный</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:IdentifierCategoriesCust">
			<xs:pattern value="(1((0[1-9])|(1[0-5])|(2[1245689])|(3[0]))[0-9a-zA-Zа-яА-Я]{19})|(200\d{14}[A-Z0-9]{2}\d{3})|300[0-9a-zA-Zа-яА-Я]{19}|(4[0]{9}\d{12})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PayerNameType">
		<xs:annotation>
			<xs:documentation>Наименование плательщика. Строка длиной до 160 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:maxLength value="2000"/>
			<xs:pattern value="[\S\t ]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PaymentIdType">
		<xs:annotation>
			<xs:documentation>УИП</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="1\d{15}((0[1-9]|[12][0-9]|3[01])(0[1-9]|1[012])\d{4})\d{8}">
				<xs:annotation>
					<xs:documentation>Структура УИП для кредитных организаций</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="2\d{4}0{11}((0[1-9]|[12][0-9]|3[01])(0[1-9]|1[012])\d{4})\d{8}">
				<xs:annotation>
					<xs:documentation>Структура УИП для ТОФК</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="3[a-f0-9]{6}((0[1-9]|[12][0-9]|3[01])(0[1-9]|1[012])\d{4})\d{17}">
				<xs:annotation>
					<xs:documentation>Структура УИП для остальных участников</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="\w{32}"/>
			<xs:pattern value="PaymentNotLoaded"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QPayerIdentifierType">
		<xs:annotation>
			<xs:documentation>Идентификатор плательщика в квитанции</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:IdentifierCategoriesCust">
			<xs:maxLength value="22"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReasonType">
		<xs:annotation>
			<xs:documentation>Основание изменения. Строка длиной до 512 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:maxLength value="512"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RecallCodeType">
		<xs:annotation>
			<xs:documentation>Отзыв/возврат исполнительного документа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:IdentifierCategoriesCust">
			<xs:length value="1"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Отзыв исполнительного документа</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Возврат исполнительного документа</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RefundIdType">
		<xs:annotation>
			<xs:documentation>Уникальный идентификатор возврата</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:pattern value="\d{8}((0[1-9]|[12][0-9]|3[01])(0[1-9]|1[012])\d{4})\d{9}"/>
			<xs:pattern value="^6\d{4}((0[1-9]|[12][0-9]|3[01])(0[1-9]|1[012])\d{4})\d{19}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RoutingCodeType">
		<xs:annotation>
			<xs:documentation>Код маршрутизации участника для предоставления информации по ВС с табличной маршрутизацией </xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Text1-1000TypePattern">
		<xs:annotation>
			<xs:documentation>Строка длиной до 1000 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:minLength value="1"/>
			<xs:maxLength value="1000"/>
			<xs:pattern value="\S+([\S\s]*\S+)*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Text1-100TypePattern1">
		<xs:annotation>
			<xs:documentation>Строка длиной до 100 символов с паттерном \S+([\S\s]*\S+)*</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
			<xs:pattern value="\S+([\S\s]*\S+)*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Text1-150TypePattern">
		<xs:annotation>
			<xs:documentation>Строка длиной от 1 до 150 символов с паттерном</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:TextCategoriesCust">
			<xs:minLength value="1"/>
			<xs:maxLength value="150"/>
			<xs:pattern value="\S+([\S\s]*\S+)*"/>
