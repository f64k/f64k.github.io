<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.24.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:enrq="urn:customs.ru:Information:SQDocuments:ExportNoticeRequest:5.24.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:CategoryCust="urn:customs.ru:Categories:3.0.0" targetNamespace="urn:customs.ru:Information:SQDocuments:ExportNoticeRequest:5.24.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.24.0.0">
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.24.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:Categories:3.0.0" schemaLocation="CategoriesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Запрос на предоставление из ГИС ГМП уведомлений по подписке</xs:documentation>
	</xs:annotation>
	<xs:element name="ExportNoticeRequest" type="enrq:ExportNoticeRequestType">
		<xs:annotation>
			<xs:documentation>Запрос на предоставление из ГИС ГМП уведомлений по подписке</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ExportNoticeRequestType">
		<xs:annotation>
			<xs:documentation>Запрос на предоставление из ГИС ГМП уведомлений по подписке</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:BaseDocType">
				<xs:sequence>
					<xs:element name="Id" type="clt_ru:Text50Type">
						<xs:annotation>
							<xs:documentation>Идентификатор запроса</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Timestamp" type="clt_ru:DateTimeCustType">
						<xs:annotation>
							<xs:documentation>Дата и время формирования сообщения</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Destination" type="enrq:DestinationType">
						<xs:annotation>
							<xs:documentation>Идентификаторы получателя уведомлений по подписке</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="NoticeCharge" type="enrq:NoticeChargeType">
							<xs:annotation>
								<xs:documentation>Уведомления о начислении, уточнении или аннулировании начисления</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeChargeExecutive" type="enrq:NoticeChargeExecutiveType">
							<xs:annotation>
								<xs:documentation>Уведомления о неуплате начисления в установленный законодательством срок</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticePayment" type="enrq:NoticePaymentType">
							<xs:annotation>
								<xs:documentation>Уведомления о поступившем платеже, уточнении или аннулировании платежа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeQuittance" type="enrq:NoticeQuittanceType">
							<xs:annotation>
								<xs:documentation>Уведомления о формировании квитанции</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeRuling" type="enrq:NoticeRulingType">
							<xs:annotation>
								<xs:documentation>Уведомление о поступившем извещении о постановлении</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeIncome" type="enrq:NoticeIncomeType">
							<xs:annotation>
								<xs:documentation>Уведомления о поступившем зачислении, уточнении или аннулировании зачисления</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeParticipant" type="enrq:NoticeParticipantType">
							<xs:annotation>
								<xs:documentation>Уведомление об изменении или регистрации косвенных участников взаимодействия</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeAcknowledgement" type="enrq:NoticeAcknowledgementType">
							<xs:annotation>
								<xs:documentation>Уведомление о принудительном квитировании</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeRefund" type="enrq:NoticeRefundType">
							<xs:annotation>
								<xs:documentation>Уведомление о поступившем возврате, уточнении или аннулировании возврата</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NoticeClarification" type="enrq:NoticeClarificationType">
							<xs:annotation>
								<xs:documentation>Уведомление о поступлении уточнения  вида и принадлежности платежа</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1009332E">
					<xs:annotation>
						<xs:documentation>Идентификатор вида документа</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>Локальные прикладные типы</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AccDocType">
		<xs:annotation>
			<xs:documentation>Сведения о документе</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AccDocNo" type="enrq:Text1-15TypePattern1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 3. Для частичного платежа поле номер 40:
                        Номер платежного документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AccDocDate" type="clt_ru:DateCustType">
				<xs:annotation>
					<xs:documentation>Поле номер 4. Для частичного платежа поле номер 41:
                        Дата платежного документа</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccPayerType">
		<xs:annotation>
			<xs:documentation>Сведения о плательщике в организации, принявшей платеж</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PayerIdentifier" type="enrq:PayerIdentifierType">
				<xs:annotation>
					<xs:documentation>Идентификатор плательщика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PayerName" type="enrq:PayerNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 8: Плательщик</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PayerAccount" type="clt_ru:AccountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 9:
                        Номер счета плательщика (при наличии) в организации, принявшей платеж</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalDataType">
		<xs:annotation>
			<xs:documentation>Дополнительные сведения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="enrq:Text1-100TypePattern2">
				<xs:annotation>
					<xs:documentation>Наименование поля</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Value" type="enrq:Text1-255TypePattern2">
				<xs:annotation>
					<xs:documentation>Значение поля</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApproverType">
		<xs:annotation>
			<xs:documentation>Информация об утверждении постановления об отказе в возбуждении ИП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PositionCode" type="enrq:Text1-25Type">
				<xs:annotation>
					<xs:documentation>Код должности СПИ/должностного лица ФССП России, утвердившего документ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PositionName" type="enrq:Text1-255TypePattern">
				<xs:annotation>
					<xs:documentation>Наименование должности лица, утвердившего документ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PersonApproving" type="enrq:Text1-1000TypePattern">
				<xs:annotation>
					<xs:documentation>ФИО лица, утвердившего документ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ApprovalDate" type="clt_ru:DateCustType">
				<xs:annotation>
					<xs:documentation>Дата утверждения</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AssignInfoType">
		<xs:annotation>
			<xs:documentation>Сведения о полномочиях участника</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="enrq:ParticipantType">
				<xs:sequence>
					<xs:element name="AssigAuthorutyInfo" type="enrq:AuthorutyType">
						<xs:annotation>
							<xs:documentation>Сведения о полномочие правопреемника</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AuthorutyType">
		<xs:annotation>
			<xs:documentation>Сведения о полномочиях участника</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Authority" type="enrq:AuthorityType">
				<xs:annotation>
					<xs:documentation>Полномочие участника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StatusAuthority" type="clt_ru:CodeVariable2Type">
				<xs:annotation>
					<xs:documentation>Статус полномочия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RegisterAuthority" type="clt_ru:DateCustType">
				<xs:annotation>
					<xs:documentation>Дата регистрация полномочия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChangeAuthority" type="clt_ru:DateCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата смены статуса полномочия</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AuthorutyInfoType">
		<xs:annotation>
			<xs:documentation>Сведения о полномочиях участника</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="enrq:AuthorutyType">
				<xs:sequence>
					<xs:element name="AssignInfo" type="enrq:AssignInfoType" minOccurs="0" maxOccurs="10">
						<xs:annotation>
							<xs:documentation>Сведение о правопреемнике</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BankType">
		<xs:annotation>
			<xs:documentation>Данные ТОФК, структурных подразделений кредитных организаций или подразделений Банка России</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="enrq:Text1-160TypePattern" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 13 для получателя средств. Поле номер 10 для организации, принявшей платеж.
                        Наименование ТОФК, структурного подразделения кредитной организации или подразделения Банка России, в котором открыт счет.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BIK" type="clt_ru:BICIDType">
				<xs:annotation>
					<xs:documentation>Поле номер 14 для получателя средств. Поле номер 11 для организации, принявшей платеж.      БИК ТОФК, структурного подразделения кредитной организации или подразделения Банка России, в котором открыт счет</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CorrespondentBankAccount" type="clt_ru:AccountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 15 для получателя средств. Поле номер 12 для организации, принявшей платеж.
                        Номер единого казначейского счета или корреспондентского счета кредитной организации, открытый в подразделении Банка России.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BudgetIndexType">
		<xs:annotation>
			<xs:documentation>Сведения о платеже</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Status" type="clt_ru:Code2Type">
				<xs:annotation>
					<xs:documentation>Поле номер 101: Статус плательщика - реквизит 101 Распоряжения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaytReason" type="enrq:PaytReasonTypePatterns">
				<xs:annotation>
					<xs:documentation>Поле номер 106: Показатель основания платежа - реквизит 106 Распоряжения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPeriod" type="enrq:TaxPeriodType">
				<xs:annotation>
					<xs:documentation>Поле номер 107:
                        Показатель налогового периода или код таможенного органа, осуществляющего в соответствии с законодательством РФ функции по выработке государственной политики и нормативному регулированию, контролю и надзору в области таможенного дела – реквизит 107 Распоряжения.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxDocNumber" type="enrq:TaxDocNumberType">
				<xs:annotation>
					<xs:documentation>Поле номер 108: Показатель номера документа - реквизит 108 Распоряжения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxDocDate" type="enrq:TaxDocDateType">
				<xs:annotation>
					<xs:documentation>Поле номер 109: Показатель даты документа - реквизит 109 Распоряжения</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ChargeDetailsType">
		<xs:annotation>
			<xs:documentation>Расширенная информация о подлежащей к уплате сумме средств</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AmountBalance" type="enrq:LongType">
				<xs:annotation>
					<xs:documentation>Сумма, отражающая остаток денежных средств налогоплательщика на ЕНС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmountDebt" type="enrq:LongType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общая подлежащая к уплате сумма средств с наступившим сроком уплаты (задолженность)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmountUpcoming" type="enrq:LongType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общая подлежащая к уплате сумма средств с ненаступившим сроком уплаты (предстоящие платежи)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DetailsInfo" type="enrq:DetailsInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Расширенная информация о подлежащей уплате сумме средств</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ChangeStatusInfoType">
		<xs:annotation>
			<xs:documentation>Сведения об изменении статуса</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Meaning" type="enrq:MeaningType">
				<xs:annotation>
					<xs:documentation>Статус, отражающий изменение данных:
                        1 - новый
                        2 - уточнение
                        3 - аннулирование
                        4 - деаннулировании</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Reason" type="enrq:ReasonType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основание изменения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChangeDate" type="clt_ru:DateTimeCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата и время уточнения информации</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ClaimerType">
		<xs:annotation>
			<xs:documentation>Информация о взыскателе</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ClaimerType" type="enrq:ClaimerTypeType">
				<xs:annotation>
					<xs:documentation>Поле номер 1200: Тип взыскателя</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClaimerAdr" type="enrq:Text1-200TypePattern">
				<xs:annotation>
					<xs:documentation>Поле номер 1107: Адрес взыскателя</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClaimerName" type="enrq:Text1-1000TypePattern">
				<xs:annotation>
					<xs:documentation>Поле номер 1203: Взыскатель</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ClarificationType">
		<xs:annotation>
			<xs:documentation>Сведения об уточнениях вида и принадлежности платежа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ClarificationId" type="enrq:ClarificationIdType">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор уточнения вида и принадлежности платежа (УВПП)
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DebtorInfoType">
		<xs:annotation>
			<xs:documentation>Информация о должнике</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DebtorName" type="enrq:Text1-255TypePattern">
				<xs:annotation>
					<xs:documentation>Поле номер 1202: Должник</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorAdr" type="enrq:Text1-200TypePattern">
				<xs:annotation>
					<xs:documentation>Поле номер 1122: Адрес должника</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DebtorType">
		<xs:annotation>
			<xs:documentation>Сведения о должнике</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DebtorType" type="clt_ru:CodeVariable4Type">
				<xs:annotation>
					<xs:documentation>Поле номер 1121: Тип должника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorName" type="enrq:Text1-1000TypePattern">
				<xs:annotation>
					<xs:documentation>Поле номер 1202: Должник</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorAdr" type="enrq:Text1-200Type">
				<xs:annotation>
					<xs:documentation>Поле номер 1122: Адрес должника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorAdrFakt" type="enrq:Text1-200Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 1123: Адрес должника фактический</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorCountryCode" type="clt_ru:Code3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 1124: Код страны принадлежности должника по Общероссийскому
                        классификатору стран мира
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorRegPlace" type="enrq:Text1-150TypePattern" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 1125: Место регистрации должника - индивидуального предпринимателя
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorBirthDate" type="clt_ru:DateCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 1126: Дата рождения должника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorGender" type="enrq:DebtorGenderType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Поле номер 1127: Пол должника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtorBirthPlace" type="enrq:Text1-100TypePattern1" minOccurs="0">
