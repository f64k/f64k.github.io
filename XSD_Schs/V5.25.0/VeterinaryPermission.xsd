<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.24.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:verp="urn:customs.ru:Information:SertifDocuments:VeterinaryPermission:5.25.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:customs.ru:Information:SertifDocuments:VeterinaryPermission:5.25.0" elementFormDefault="qualified" version="5.25.0.0">
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.24.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Ответ на запрос ветеринарного разрешения</xs:documentation>
	</xs:annotation>
	<xs:element name="VeterinaryPermission" type="verp:VeterinaryPermissionType">
		<xs:annotation>
			<xs:documentation>Ответ на запрос ветеринарного разрешения</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="VeterinaryPermissionType">
		<xs:annotation>
			<xs:documentation>Ответ на запрос ветеринарного разрешения</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:BaseDocType">
				<xs:sequence>
					<xs:element name="UUID" type="clt_ru:DocumentIDType">
						<xs:annotation>
							<xs:documentation>Идентификатор цепочки обмена сообщениями</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="responseCode" type="clt_ru:Text40Type" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Код ответа сервера</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="veterinaryPermitCount" type="clt_ru:Text40Type" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Количество ветеринарных разрешений</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VeterinaryPermit" type="verp:VeterinaryPermitType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Ветеринарное разрешение</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1009415E">
					<xs:annotation>
						<xs:documentation>Идентификатор вида документа</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>Локальные прикладные типы</xs:documentation>
	</xs:annotation>
	<xs:complexType name="VeterinaryPermitType">
		<xs:annotation>
			<xs:documentation>Ветеринарное разрешение</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PermitUUID" type="clt_ru:DocumentIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа в системе РСХН</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Series" type="clt_ru:FreeText500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Серия документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Number" type="clt_ru:FreeText500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="clt_ru:DateCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="clt_ru:FreeText250Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Название документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="YearPermit" type="clt_ru:DateYearCustType">
				<xs:annotation>
					<xs:documentation>Год, на который выдано разрешение</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Decision" type="clt_ru:Text10Type">
				<xs:annotation>
					<xs:documentation>Решение (разрешение или запрет)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CentralDepartment" type="verp:DepartmentType">
				<xs:annotation>
					<xs:documentation>ГУВ страны ТС, оформившее разрешения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vetDepartment" type="verp:DepartmentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ветеринарное управления субъекта страны ТС, в который следует груз</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SignedBy" type="verp:SignedByType">
				<xs:annotation>
					<xs:documentation>Должностное лицо, ответственное за выдачу разрешения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isGeneral" type="clt_ru:IndicatorType">
				<xs:annotation>
					<xs:documentation>Является ли решение генеральным (общим)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransferType" type="clt_ru:Numeric1Type">
				<xs:annotation>
					<xs:documentation>Тип операции: 1 - Ввоз (импорт на территорию ТС); 2 - Вывоз (экспорт из ТС в третьи страны); 3 - Транзит через территорию ТС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WhichProductType" type="clt_ru:Numeric1Type">
				<xs:annotation>
					<xs:documentation>Тип продукции: 1 - Мясо и мясопродукты; 2 - Корма и кормовые добавки; 3 - Живые животные; 4 - Лекарственные средства; 5 - Пищевые продукты; 6 - Непищевые продукты и другое; 7 - Рыба и морепродукты; 8 - Продукция, не требующая разрешения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransferPurpose" type="verp:TransferPurposeType">
				<xs:annotation>
					<xs:documentation>Цель ввоза/вывоза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BusinessEntity" type="verp:BusinessEntityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Заявитель</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginCountry" type="verp:CountryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна происхождения груза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ImportCountry" type="verp:CountryType">
				<xs:annotation>
					<xs:documentation>Страна-импортер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExportCountry" type="verp:CountryType">
				<xs:annotation>
					<xs:documentation>Страна-экспортер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Supplier" type="clt_ru:FreeText500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Предприятие-поставщий животных</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Recipient" type="clt_ru:FreeText500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Получатель</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Mediator" type="clt_ru:FreeText500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Посредник доставки груза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomsClearanceRegion" type="verp:RegionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Регион полного таможенного оформления</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StoragePoint" type="verp:StoragePointType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Место хранения груза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipmentPoint" type="clt_ru:FreeText500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Место доставки груза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BipListRestricted" type="clt_ru:IndicatorType">
				<xs:annotation>
					<xs:documentation>TRUE --  список допустимых пункты пропуска ограничен.
                        Перечень приводится в элементе borderInspectionPointList.
                        FALSE -- список допустимых пунктов пропуска не ограничен.
                        Допускается транспорт груза через любой пункт пропуска ТС.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BorderInspectonPointList" type="verp:BorderInspectonPointListType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Список допустимых пунктов пропуска</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransportTypeList" type="verp:TransportTypeListType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Список допустимых типов транспорта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CargoList" type="verp:CargoListType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Список грузов</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransferConditioinList" type="verp:TransferConditioinListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Список условий транспортировки груза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AddtonalInfo" type="clt_ru:FreeText500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TransferConditioinListType">
		<xs:annotation>
			<xs:documentation>Список условий транспортировки груза</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Conditon" type="clt_ru:FreeText500Type" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Условия транспортировки груза</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
