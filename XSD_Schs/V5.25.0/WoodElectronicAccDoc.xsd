      <xs:element name="Name" type="RUSclt_ru:Text100Type">
        <xs:annotation>
          <xs:documentation>Имя</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Patronymic" type="RUSclt_ru:Text100Type" minOccurs="0">
