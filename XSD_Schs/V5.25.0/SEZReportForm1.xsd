          <xs:element name="GoodsGU" type="sez_rf1:GoodsGUType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Сведения о товарах, товары ЕАЭС</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
