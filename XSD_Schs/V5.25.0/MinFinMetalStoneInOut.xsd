        </xs:annotation>
      </xs:element>
      <xs:element name="PersonName" type="sq_clt:FreeText4000Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Ф.И.О</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Number" type="clt_ru:FreeText500Type">
