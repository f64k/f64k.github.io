<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.24.0" xmlns:cltTrans_ru="urn:customs.ru:Information:TransportDocuments:TransportCommonLeafTypesCust:5.14.3" xmlns:catTrans_ru="urn:customs.ru:Information:TransportDocuments:TransportCommonAgregateTypesCust:5.24.0" xmlns:rwb="urn:customs.ru:Information:TransportDocuments:Railway:RailwayBill:5.24.0" targetNamespace="urn:customs.ru:Information:TransportDocuments:Railway:RailwayBill:5.24.0" elementFormDefault="qualified" version="5.24.0.0">
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.24.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:TransportDocuments:TransportCommonLeafTypesCust:5.14.3" schemaLocation="TransportCommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:TransportDocuments:TransportCommonAgregateTypesCust:5.24.0" schemaLocation="TransportCommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Международная железнодорожная накладная по форме СМГС</xs:documentation>
  </xs:annotation>
  <xs:element name="RailwayBill" type="rwb:RailwayBillType" />
  <xs:complexType name="RailwayBillType">
    <xs:annotation>
      <xs:documentation>Международная железнодорожная накладная по форме СМГС </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="RWBMain_ID" type="xs:decimal" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Идентификатор цепочки документов </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Number" type="clt_ru:DocumentNumberType">
            <xs:annotation>
              <xs:documentation>Номер отправки (графа 29 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RegistrationDocument" type="cat_ru:DocumentBaseType">
            <xs:annotation>
              <xs:documentation>Информация о регистрации СМГС </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ConsignorNotice" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Особые заявления отправителя (графа 3 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ConsignorOptionNotice" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Отметки отправителя, необязательные для железной дороги</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PlacesQuantity" type="clt_ru:Quantity11Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Общее количество грузовых мест отправки товаров накладной.  В случае транспортировке "Навалом" или "Налив"  не заполняется.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GrossWeightQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
              <xs:documentation>Общий вес товаров, определенный отправителем, брутто (кг)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DepartureModeDescription" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Описание рода отправки </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LoaderModeDescription" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Описание кем погружено  (графа 20 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AdditionalSheetQuantity" type="clt_ru:Count5Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Количество приложенных дополнительных листов к накладной</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CargoValue" type="clt_ru:AmountType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Объявленная ценность груза, руб.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CustomsNotice" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Отметки таможенных органов</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="WeightDefinitionModeDescription" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Cпособ определения веса (графа 21 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignor" type="rwb:RBOrganizationType">
            <xs:annotation>
              <xs:documentation>Сведения о грузоотправителе (графа 1 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignee" type="rwb:RBOrganizationType">
            <xs:annotation>
              <xs:documentation>Сведения о грузополучателе (графа 4 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DepartureStation" type="rwb:StationType">
            <xs:annotation>
              <xs:documentation>Ж/д станция и дорога отправления (графа 2 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DestinationStation" type="rwb:RailwayType">
            <xs:annotation>
              <xs:documentation>Ж/д станция и дорога назначения (графа 5 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BorderStation" type="rwb:StationType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Пограничные ж/д станции и дорога назначения (графа 6 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Contract" type="cat_ru:DocumentBaseType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Реквизиты договора на поставку, заключенного между экспортером и импортером (графа 25 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RWBGoods" type="rwb:RWBGoodsType" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Сведения о товарах накладной</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExchangePallet" type="rwb:ExchangePalletType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения об обменных поддонах </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="WorkTool" type="rwb:WorkToolType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Описание перевозочных приспособлений</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Container" type="rwb:ContainerType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Сведения о контейнере</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Auto" type="rwb:AutoType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>ИТЕ или АТС </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ConsignorDocument" type="rwb:RWDocumentType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Документы, приложенные отправителем (графа 24 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Carriage" type="rwb:CarriageType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Данные об используемых вагонах (графы 7-14 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Seal" type="rwb:SealType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Количество и знаки пломб,  наложенных на вагон или контейнер (графа 19 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Carrier" type="rwb:CarrierType" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Перевозчики (графа 22 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PaidRailwayCodeName" type="rwb:PaidRailwayCodeNameType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Наименования и коды транзитных железнодорожных дорог,  уплата провозных платежей (графа 23 СМГС )</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Mark" type="rwb:MarkType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Отметки о прохождении товарами таможенного и иных видов государственного контроля.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ContractCarrierDate" type="rwb:StampType">
            <xs:annotation>
              <xs:documentation>Дата заключения договора перевозки (графа 26 СМГС) </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ArrivalDate" type="rwb:StampType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дата прибытия (графа 27 СМГС) </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CarrierNotice" type="clt_ru:FreeText500Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Отметки перевозчика (графа 30 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CommerceAkt" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Коммерческий акт (графа 31 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Prolongation" type="clt_ru:FreeText250Type" minOccurs="0" maxOccurs="2">
            <xs:annotation>
              <xs:documentation>Удлинение срока доставки (графа 32 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Stamp33" type="rwb:StampType" minOccurs="0" maxOccurs="12">
            <xs:annotation>
              <xs:documentation>Отметки о передаче груза (графа 33 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Stamp34" type="rwb:StampType" minOccurs="0" maxOccurs="12">
            <xs:annotation>
              <xs:documentation>Отметки о проследовании погран станций (графа 34 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Stamp35" type="rwb:StampType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Уведомление о прибытии груза (графа 35 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueStamp" type="rwb:StampType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Штамп выдачи (графа 36 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueMarks" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Отметки о выдаче груза (графа 36 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CarrierCharge" type="rwb:ChargeType" minOccurs="0" maxOccurs="6">
            <xs:annotation>
              <xs:documentation>Разделы по расчету провозных платежей (графы 37-59)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Amount60" type="clt_ru:AmountType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сумма всего в валюте тарифа с отправителя (графа 60 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Amount61" type="clt_ru:AmountType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сумма всего в валюте расчетов с отправителя (графа 61 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Amount62" type="clt_ru:AmountType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сумма всего в валюте тарифа с получателя (графа 62 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Amount63" type="clt_ru:AmountType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сумма итого в валюте расчетов с получателя (графа 63 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CalculationNotice" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Отметки для исчисления и взымания провозных платежей (графа 64 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AdditionPayment" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дополнительно взыскать с отправителя (графа 65 СМГС)</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1003401E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные типы.</xs:documentation>
  </xs:annotation>
  <xs:complexType name="CarriageType">
    <xs:annotation>
      <xs:documentation>Данные об используемых вагонах </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CarriageID" type="clt_ru:TransportMeansIDType">
        <xs:annotation>
          <xs:documentation>Номер вагона  (графа 7 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RWName" type="clt_ru:Text50Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование железной дороги (графа 7 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerName" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование собственника вагона (графа 7 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerType" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Вагон предоставлен (О- отправителем, П-перевозчиком)
(графа 8 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Power" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Подъемная сила вагона в тоннах (графа 9 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AxisQuantity" type="clt_ru:Quantity11Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество осей вагона (графа 10 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Tare" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Вес тары в кг (графа 11 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsWeight" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Вес груза после перегрузки (графа 13 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Caliber" type="clt_ru:Text50Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Тип цистерны (калибр котла) Г(рафа 12 СМГС)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ContainerType">
    <xs:annotation>
      <xs:documentation>Сведения о контейнере</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RailwayCode" type="cltTrans_ru:RailwayCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код железнодорожной дороги - владельца контейнера. Не указывается, если контейнер не принадлежит железным дорогам</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContainerID" type="clt_ru:TransportMeansIDType">
        <xs:annotation>
          <xs:documentation>Номер контейнера</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContainerKind" type="clt_ru:PackageCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Тип контейнера в соответствии с классификатором видов груза, упаковки и упаковочных материалов </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContainerModeDescription" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Описание вида контейнера</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContainerCapacity" type="clt_ru:Quantity11Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Емкость контейнера</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContainerCapacityUnitQualifierCode" type="clt_ru:MeasureUnitQualifierNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Название единицы измерения емкости контейнера</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Tare" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Масса тары контейнера в кг</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsWeight" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Масса груза в контейнере в кг</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PlacesQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество мест груза в вагоне</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PocketQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество транспортных пакетов в вагоне</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContainerLength" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Длина контейнера в футах</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DTInfoType">
    <xs:annotation>
      <xs:documentation>Сведения из декларации на товары</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="GoodsNumeric" type="clt_ru:Count3Type">
        <xs:annotation>
          <xs:documentation>Номер товара по ДТ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DTNumber" type="cat_ru:GTDIDType">
        <xs:annotation>
          <xs:documentation>Регистрационный номер декларации на товары</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ExchangePalletType">
    <xs:annotation>
      <xs:documentation>Сведения об обменных поддонах</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FlatPalletQuantity" type="clt_ru:Quantity11Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество плоских поддонов </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BoxPalletQuantity" type="clt_ru:Quantity11Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество поддонов ящичного типа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExchangeMode" type="cltTrans_ru:ExchangeModeCodeType">
        <xs:annotation>
          <xs:documentation>Код режим обмена поддонов. </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InfoPayerType">
    <xs:annotation>
      <xs:documentation>Сведения о плательщике</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PayerName" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Наименование плательщика</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayerCode" type="clt_ru:Code2Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код плательщика</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MarkType">
    <xs:annotation>
      <xs:documentation>Отметки о прохождении товарами таможенного и иных видов государственного контроля.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MarkSing" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Признак наличия отметки</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarkKind" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Тип отметки</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ResultControl" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Результат прохождения контроля</xs:documentation>
        </xs:annotation>
