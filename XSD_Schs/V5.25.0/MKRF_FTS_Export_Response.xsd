      <xs:documentation>Строка до 2000 символов</xs:documentation>
    </xs:annotation>
    <xs:restriction base="RUSclt_ru:FreeText4000Type">
      <xs:maxLength value="2000" />
    </xs:restriction>
  </xs:simpleType>
