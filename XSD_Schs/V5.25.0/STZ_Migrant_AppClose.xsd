					<xs:documentation>Год вывоза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ManufactureMonth" type="clt_ru:Count2Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Месяц вывоза</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GoodsExportDetailsType">
		<xs:annotation>
			<xs:documentation>Вывоз товаров для личного пользования</xs:documentation>
		</xs:annotation>
		<xs:sequence>
