<?xml version="1.0" encoding="UTF-8"?>
<!--Приказ ФТС России № 131 от 28 января 2019 года Об утверждении Административного регламента Федеральной таможенной службы по предоставлению государственной услуги по ведению таможенного реестра объектов интеллектуальной собственности -->
<xs:schema xmlns:ipap="urn:customs.ru:TROISIntellectualProperty:5.25.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.24.0" targetNamespace="urn:customs.ru:TROISIntellectualProperty:5.25.0" elementFormDefault="qualified" version="5.25.0">
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.24.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Государственная услуга по ведению таможенного реестра объектов интеллектуальной собственности</xs:documentation>
	</xs:annotation>
	<xs:element name="TROISIntellectualProperty" type="ipap:TROISIntellectualPropertyType">
		<xs:annotation>
			<xs:documentation>Государственная услуга по ведению таможенного реестра объектов интеллектуальной собственности</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="TROISIntellectualPropertyType">
		<xs:annotation>
			<xs:documentation>Государственная услуга по ведению таможенного реестра объектов интеллектуальной собственности</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:BaseDocType">
				<xs:sequence>
					<xs:element name="ProcessingPersonalData" type="clt_ru:IndicatorType" minOccurs="1">
						<xs:annotation>
							<xs:documentation>Согласие на обработку персональных данных: 0 - Нет, 1 - Да</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationSystemInfo" type="ipap:ApplicationSystemInfoType">
						<xs:annotation>
							<xs:documentation>Системные данные из ЕПГУ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Service" type="ipap:ServiceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Услуга</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Additionally" type="ipap:AdditionallyType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Доп. информация</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InfoNumberAppsheets" type="ipap:InfoNumberAppsheetsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Количество листов приложений</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="DocumentModeID" type="clt_ru:DocumentIDType" use="required" fixed="1006375E">
					<xs:annotation>
						<xs:documentation>Идентификатор вида документа</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>Локальные прикладные комплексные типы</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AdditionalDataType">
		<xs:annotation>
			<xs:documentation>Дополнительные данные</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="UserType" type="ipap:UserTypeType">
				<xs:annotation>
					<xs:documentation>Тип заявителя (из справочника "Типы заявителей")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherDocuments" type="ipap:OtherDocumentInfoType" minOccurs="0" maxOccurs="20">
				<xs:annotation>
					<xs:documentation>Дополнительные документы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionallyType">
		<xs:annotation>
			<xs:documentation>Доп. информация</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FIO" type="clt_ru:FreeText250Type" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>ФИО</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation>Блок полей адреса организации / физического лица согласно КЛАДР/ФИАС либо свободная строка адреса</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FullAddress" type="ipap:stringMaxLength-1000" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Адрес</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Index" type="ipap:stringMaxLength-20">
				<xs:annotation>
					<xs:documentation>Почтовый индекс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Region" type="ipap:stringMaxLength-250">
				<xs:annotation>
					<xs:documentation>Область (регион, штат, провинция и т.п.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Area" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Район</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="City" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Город</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CityArea" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Внутригородской район</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Place" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Населенный пункт</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Street" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Улица</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalArea" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Доп. территория</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalStreet" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Улица на доп. территории</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="House" type="ipap:stringMaxLength-20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Building1" type="ipap:stringMaxLength-20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Корпус</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Building2" type="ipap:stringMaxLength-20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Строение</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Apartment" type="ipap:stringMaxLength-20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер квартиры, офиса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalInformation" type="ipap:stringMaxLength-250" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация по адресу</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicantInfoType">
		<xs:annotation>
			<xs:documentation>Данные о заявителе</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="UserType" type="ipap:UserTypeType">
				<xs:annotation>
					<xs:documentation>Тип заявителя</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryRegInfo" type="ipap:CountryInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна регистрации лица</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="LegalEntity" type="ipap:LegalEntityType">
					<xs:annotation>
						<xs:documentation>Юридическое лицо</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PhysicalPerson" type="ipap:PhysicalPersonType">
					<xs:annotation>
						<xs:documentation>Физическое лицо</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IP" type="ipap:IPPersonType">
					<xs:annotation>
						<xs:documentation>Индивидуальный предприниматель</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="RightHolderCheck" type="ipap:RightHolderCheckType">
				<xs:annotation>
					<xs:documentation>Заявитель является</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsTrustee" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Заявитель является доверенным лицом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Confidence" type="ipap:ConfidenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Доверенность</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicationInfoType">
		<xs:annotation>
			<xs:documentation>Общая информация о заявлении</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ApplicantInfo" type="ipap:ApplicantInfoType">
				<xs:annotation>
					<xs:documentation>Заявитель</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="RegistryInclusion" type="ipap:RegistryInclusionType">
					<xs:annotation>
						<xs:documentation>Данные о включении объектов интеллектуальной собственности в таможенный реестр</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RegistryCertificateChanging" type="ipap:RegistryInfoChangingType">
					<xs:annotation>
						<xs:documentation>Данные о внесении изменений в заявление или в таможенный реестр</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RegistryExclusion" type="ipap:RegistryExclusionType">
					<xs:annotation>
						<xs:documentation>Данные об исключении объекта интеллектуальной собственности из таможенного реестра</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RegistryRenewal" type="ipap:RegistryRenewalType">
					<xs:annotation>
						<xs:documentation>Данные о продлении срока включения объекта интеллектуальной собственности в таможенный реестр</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicationSystemInfoType">
		<xs:annotation>
			<xs:documentation>Системные данные из ЕПГУ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Код транзакции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ServiceCode" type="ipap:stringMaxLength-20">
				<xs:annotation>
					<xs:documentation>Код цели услуги</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ApplicationSourceSystemID" type="clt_ru:DocumentIDType">
				<xs:annotation>
					<xs:documentation>Идентификатор заявления на ЕПГУ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Department" type="ipap:CustomsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Подразделение, в которое предоставляется заявка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ApplicationID" type="clt_ru:DocumentIDType">
				<xs:annotation>
					<xs:documentation>Номер заявления на АПС "Электронные Госуслуги"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CreationDate" type="clt_ru:DateCustType">
				<xs:annotation>
					<xs:documentation>Дата поступления запроса в АПС "Электронные Госуслуги"</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ArchDocType">
		<xs:annotation>
			<xs:documentation>Ссылка на документ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ArchID" type="clt_ru:DocumentIDType">
				<xs:annotation>
					<xs:documentation>Идентификатор архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArchDocID" type="clt_ru:DocumentIDType">
				<xs:annotation>
					<xs:documentation>Идентификатор документа архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TroisDoc" type="ipap:TroisDocType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документ в соответствии со справочником документов ТРОИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChangeAction" type="clt_ru:LetterIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак изменения: 0 - без изменений, 1 - изменение</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConfidenceDetailsType">
		<xs:annotation>
			<xs:documentation>Доверенность. Базовое описание.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ArchID" type="clt_ru:DocumentIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArchDocID" type="clt_ru:DocumentIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TroisDoc" type="ipap:TroisDocType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документ в соответствии со справочником документов ТРОИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LetterOfAttorneyNumber" type="ipap:stringMaxLength-20">
				<xs:annotation>
					<xs:documentation>Номер доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LetterOfAttorneyDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата составления доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateUpTo" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата окончания срока действия доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Indefinitely" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Бессрочно</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Principal" type="ipap:stringMaxLength-250">
				<xs:annotation>
					<xs:documentation>Доверитель</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Agent" type="ipap:stringMaxLength-250">
				<xs:annotation>
					<xs:documentation>Доверенное лицо</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NotarizedDocumentInfo" type="ipap:NotarizedDocumentInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о нотариальной доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConfidenceType">
		<xs:annotation>
			<xs:documentation>Доверенность</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Attorney" type="ipap:ConfidenceDetailsType">
				<xs:annotation>
					<xs:documentation>Доверенность</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Retrust" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Доверенность получена в порядке передоверия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MajorAttorney" type="ipap:ConfidenceDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основная доверенность, подтверждающая полномочия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChangeAction" type="clt_ru:LetterIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак изменения: 0 - без изменений, 1 - изменение</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConsentDocumentInfoType">
		<xs:annotation>
			<xs:documentation>Документ, подтверждающий согласие правообладателя на использования ОИС</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ArchID" type="clt_ru:DocumentIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArchDocID" type="clt_ru:DocumentIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TroisDoc" type="ipap:TroisDocType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документ в соответствии со справочником документов ТРОИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Number" type="ipap:stringMaxLength-30">
				<xs:annotation>
					<xs:documentation>Номер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Validity" type="ipap:ValidityType">
				<xs:annotation>
					<xs:documentation>Срок действия полномочий</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateEnd" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата завершения действия</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContactInformationType">
		<xs:annotation>
			<xs:documentation>Контактная информация</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Phone" type="ipap:stringMaxLength-50" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Телефон</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Fax" type="ipap:stringMaxLength-50" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Факс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Email" type="ipap:stringMaxLength-50" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Адрес электронной почты</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CountryInfoType">
		<xs:annotation>
			<xs:documentation>Сведения о стране</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Буквенный код страны в соответствии с классификатором стран мира</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryName" type="clt_ru:CountryNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Краткое название страны в соответствии с классификатором стран мира</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsType">
		<xs:annotation>
			<xs:documentation>Код и наименование таможенного органа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OfficeName" type="ipap:stringMaxLength-250">
				<xs:annotation>
					<xs:documentation>Наименование таможенного органа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Code" type="ipap:CustomsCodeType">
				<xs:annotation>
					<xs:documentation>Код таможенного органа</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
