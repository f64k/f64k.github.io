<?xml version="1.0" encoding="UTF-8"?>
<!--Решение Коллегии Евразийской экономической комиссии от 02.04.2019 г. № 48
"О внесении изменений в структуру и формат предварительной информации о товарах, предполагаемых к ввозу на таможенную территорию Евразийского экономического союза водным транспортом" -->
<!--Решение Коллегии ЕЭК от 20.10.2020 № 130 "О внесении изменений в некоторые решения Коллегии Евразийской экономической комиссии"-->
<!--Решение Коллегии Евразийской экономической комиссии от 15.08.2023 г. № 115"О внесении изменений в структуру и формат предварительной информации о товарах, предполагаемых к ввозу на таможенную территорию Евразийского экономического союза водным транспортом-->
<xs:schema xmlns:sp="urn:customs.ru:Information:CustomsDocuments:SeaPreliminary:5.25.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.24.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:RUScat_ru="urn:customs.ru:RUSCommonAggregateTypes:5.24.0" xmlns:RUSclt_ru="urn:customs.ru:RUSCommonLeafTypes:5.21.0" xmlns:RUDECLcat="urn:customs.ru:RUDeclCommonAggregateTypesCust:5.24.0" xmlns:cltpi_ru="urn:customs.ru:Information:PriorInformation:PriorCommonLeafTypes:5.17.0" xmlns:cltESAD_cu="urn:customs.ru:CUESADCommonLeafTypes:5.17.0" xmlns:catpi_ru="urn:customs.ru:Information:PriorInformation:PriorCommonAggregateTypes:5.24.0" targetNamespace="urn:customs.ru:Information:CustomsDocuments:SeaPreliminary:5.25.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.25.0.0">
	<xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.24.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CUESADCommonLeafTypes:5.17.0" schemaLocation="CUESADCommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:RUSCommonAggregateTypes:5.24.0" schemaLocation="RUSCommonAggregateTypes.xsd"/>
	<xs:import namespace="urn:customs.ru:RUSCommonLeafTypes:5.21.0" schemaLocation="RUSCommonLeafTypes.xsd"/>
	<xs:import namespace="urn:customs.ru:Information:PriorInformation:PriorCommonLeafTypes:5.17.0" schemaLocation="PriorCommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:Information:PriorInformation:PriorCommonAggregateTypes:5.24.0" schemaLocation="PriorCommonAggregateTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:RUDeclCommonAggregateTypesCust:5.24.0" schemaLocation="RUDeclCommonAggregateTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Предварительная информация о товарах, предполагаемых к ввозу на таможенную территорию Евразийского экономического союза водным транспортом </xs:documentation>
	</xs:annotation>
	<xs:element name="SeaPreliminary" type="sp:SeaPreliminaryType">
		<xs:annotation>
			<xs:documentation>Предварительная информация о товарах, предполагаемых к ввозу на таможенную территорию Евразийского экономического союза водным транспортом </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="SeaPreliminaryType">
		<xs:annotation>
			<xs:documentation>Предварительная информация о товарах, предполагаемых к ввозу на таможенную территорию Евразийского экономического союза водным транспортом </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:BaseDocType">
				<xs:sequence>
					<xs:element name="EECEDocHeaderAddInfo" type="RUScat_ru:EECEDocHeaderAddInfoType">
						<xs:annotation>
							<xs:documentation>Дополнительные технологические реквизиты электронного документа (сведений)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ElectronicDocumentSign" type="clt_ru:Code2Type">
						<xs:annotation>
							<xs:documentation>Признак электронного документа "ЭД"</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RegNumber" type="catpi_ru:PIRegNumberType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Регистрационный номер предварительной информации, содержащей обязательный состав сведений</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RefRegNumber" type="catpi_ru:PIRegNumberType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Регистрационный номер предварительной информации, в отношении которой осуществляется повторное представление предварительной информации</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="UsageCode" type="clt_ru:Code2Type" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Код цели представления предварительной информации</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ArrivalDateTime" type="clt_ru:DateTimeCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Планируемые дата и время прибытия</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntryCheckPointDetails" type="sp:EntryCheckPointDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Место прибытия товаров на таможенную территорию Евразийского экономического союза</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DeclarantDetails" type="catpi_ru:PI_EEC_DeclarantDetailsType">
						<xs:annotation>
							<xs:documentation>Лицо, представившее предварительную информацию</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BorderTransportDetails" type="sp:BorderTransportDetailsType">
						<xs:annotation>
							<xs:documentation>Сведения о транспортном средстве, прибывающем на таможенную территорию Евразийского экономического союза</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ConsignmentDetails" type="sp:ConsignmentDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Сведения о товарной партии</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CarrierDetails" type="catpi_ru:PI_EEC_CarrierDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Сведения о перевозчике, осуществляющем ввоз товаров на таможенную территорию Евразийского экономического союза</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AgentDetails" type="sp:AgentDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Сведения о судовом агенте</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ControlledItemsDetails" type="catpi_ru:ControlledItemsDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Сведения о припасах, запасных частях и оборудовании, подлежащие указанию при предоставлении предварительной информации</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EpidemicControlDetails" type="sp:EpidemicControlDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Сведения, представляемые в целях санитарно-эпидемиологического надзора</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DiseasedPersonDetails" type="sp:DiseasedPersonDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Лицо, у которого выявлено заболевание или обнаружено подозрение на заболевание</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1006828E">
					<xs:annotation>
						<xs:documentation>Идентификатор вида документа. Код документа в Альбоме форматов</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>Локальные прикладные типы</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AffectedPortDetailsType">
		<xs:annotation>
			<xs:documentation>Сведения о заходе в порт на зараженной территории</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PlaceName" type="RUSclt_ru:Name120Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование места</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LocationCode" type="RUSclt_ru:LocationCodeType">
				<xs:annotation>
					<xs:documentation>Кодовое обозначение порта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventDate" type="clt_ru:DateCustType">
				<xs:annotation>
					<xs:documentation>Дата</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AgentDetailsType">
		<xs:annotation>
			<xs:documentation>Сведения о судовом агенте</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RUScat_ru:SubjectDetailsType">
				<xs:sequence>
					<xs:element name="AgentRepresentativeDetails" type="sp:AgentRepresentativeDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Сведения о лице, являющимся представителем агента</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AgentRepresentativeDetailsType">
		<xs:annotation>
			<xs:documentation>Сведения о лице, являющимся представителем агента</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:PersonBaseType">
				<xs:sequence>
					<xs:element name="CommunicationDetails" type="cat_ru:ContactType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Контактный реквизит должностного лица</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BorderTransportDetailsType">
		<xs:annotation>
			<xs:documentation>Сведения о транспортном средстве, прибывающем на таможенную территорию Евразийского экономического союза</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectOrdinal" type="clt_ru:Count3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>порядковый номер транспортного средства</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransportModeCode" type="clt_ru:TransportModeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код вида транспортного средства по классификатору видов транспорта и транспортировки товаров</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContainerIndicator" type="clt_ru:IndicatorType">
				<xs:annotation>
					<xs:documentation>Признак контейнерных перевозок (1 - товары перевозятся в контейнере; 0 - товары перевозятся не в контейнере)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipName" type="clt_ru:TransportMeansIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NationalityCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код страны принадлежности транспорта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipIMONumber" type="cltpi_ru:IMONumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ИМО номер судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipType" type="clt_ru:Text50Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Тип судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipTypeCode" type="clt_ru:CodeVariable4Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код типа судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrossTonnage" type="catpi_ru:UnifiedPhysicalMeasureType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Брутто регистровый тоннаж судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipDetails" type="sp:ShipDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о национальной регистрации водного судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VoyageId" type="clt_ru:CodeVariable8Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер рейса водного судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteDetails" type="sp:RouteDetailsType" minOccurs="0" maxOccurs="4">
				<xs:annotation>
					<xs:documentation>Сведения о маршруте</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransportMeansEntryPurpose" type="clt_ru:LetterIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кодовое обозначение цели ввоза ТСМП</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewQuantity" type="clt_ru:Quantity4Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Количество членов экипажа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PassengerQuantity" type="clt_ru:Quantity5Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Количество пассажиров</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Captain" type="sp:CrewDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о капитане водного судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewMemberDetails" type="sp:CrewDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о члене экипажа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PassengerDetails" type="sp:PassengerInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о пассажире</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConsignmentDetailsType">
		<xs:annotation>
			<xs:documentation>Сведения о товарной партии</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectOrdinal" type="clt_ru:Count4Type">
				<xs:annotation>
					<xs:documentation>Порядковый номер товарной партии</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UsageCode" type="clt_ru:Code2Type" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Код цели представления предварительной информации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransportDocumentDetails" type="RUScat_ru:DocBaseV4Type">
				<xs:annotation>
					<xs:documentation>Сведения о транспортном (перевозочном документе)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomsDocIdDetails" type="catpi_ru:CustomDocIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Регистрационный номер таможенной декларации, в соответствии с которой осуществлено предварительное таможенное декларирование</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomsProcedure" type="clt_ru:CUCustomsProcedureType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код типа таможенной декларации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeclarationFeatureCode" type="cltESAD_cu:CustomsDeclarationKindType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кодовое обозначение особенности таможенного декларирования товаров</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransitDirectionCode" type="clt_ru:TransitDirectionCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кодовое обозначение особенности перевозки (транспортировки) товаров при применении таможенной процедуры таможенного транзита</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransitFeature" type="clt_ru:CodeVariable3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Особенность помещения товаров под процедуру таможенного транзита (МПО - международные почтовые отправления, ФЛ - товары и (или) транспортные средства для личного пользования)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocUsageCode" type="clt_ru:CodeVariable3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кодовое обозначение использования транспортных (перевозочных), коммерческих и (или) иных документов в качестве транзитной декларации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PageQuantity" type="clt_ru:Count4Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общее количество листов в документе</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalGoodsNumber" type="clt_ru:Quantity5Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общее число товаров по транспортному (перевозочному) документу</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalPackageNumber" type="clt_ru:PakageNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общее количество грузовых мест</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DispatchCountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна отправления</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationCountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна назначения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalAmount" type="RUScat_ru:AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общая стоимость товаров</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrossWeightQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общий вес брутто товаров в товарной партии</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConsignorDetails" type="catpi_ru:PI_EEC_SubjectDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения об отправителе</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConsigneeDetails" type="catpi_ru:PI_EEC_SubjectDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о получателе</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoadingLocationDetails" type="catpi_ru:PI_EEC_CargoLocationDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о месте погрузки товаров на судно</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnloadingIndicator" type="clt_ru:IndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак выгрузки товаров в портах государства - члена Евразийского экономического союза (0 - нет; 1- да)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnLoadingLocationDetails" type="catpi_ru:PI_EEC_CargoLocationDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о месте выгрузки товаров с судна</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationCustomsOfficeCode" type="clt_ru:CustomsCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о таможенном органе назначения, указываемые при принятии решения в отношении подкарантинной продукции</xs:documentation>
				</xs:annotation>
			</xs:element>
