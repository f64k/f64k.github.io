  </xs:annotation>
  <xs:complexType name="MPODeclType">
    <xs:annotation>
      <xs:documentation>Общая информация о МПО</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MPO_Number" type="clt_ru:Code13Type">
