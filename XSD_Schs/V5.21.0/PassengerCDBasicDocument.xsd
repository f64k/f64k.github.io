          <xs:documentation>Наименование и другие отличительные признаки товара</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PresentedDocuments" type="pcd_bd:PresentedDocumentsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Номер и дата выдачи документа, подтверждающего соблюдение ограничений и орган, его выдавший</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsWeightQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Вес</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsQuantity" type="pcd_bd:QuantityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsCost" type="cat_pcd:MoveMoneyType">
        <xs:annotation>
