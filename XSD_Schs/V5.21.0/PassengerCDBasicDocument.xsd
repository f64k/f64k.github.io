<?xml version="1.0" encoding="UTF-8"?>
<!--  Решение КТС от 18.06.2010 №287  -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.21.0" xmlns:catESAD_cu="urn:customs.ru:CUESADCommonAggregateTypesCust:5.21.0" xmlns:cat_pcd="urn:customs.ru:Information:CustomsDocuments:PassengerCDCommonAggregateTypesCust:5.21.0" xmlns:pcd_bd="urn:customs.ru:Information:CustomsDocuments:PassengerCDBasicDocument:5.21.0" targetNamespace="urn:customs.ru:Information:CustomsDocuments:PassengerCDBasicDocument:5.21.0" elementFormDefault="qualified" version="5.21.0.0">
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.21.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.21.0" schemaLocation="CUESADCommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:CustomsDocuments:PassengerCDCommonAggregateTypesCust:5.21.0" schemaLocation="PassengerCDCommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Пассажирская таможенная декларация</xs:documentation>
  </xs:annotation>
  <xs:element name="PassengerCDBasicDocument" type="pcd_bd:PassengerCDBasicDocumentType" />
  <xs:complexType name="PassengerCDBasicDocumentType">
    <xs:annotation>
      <xs:documentation>Пассажирская таможенная декларация</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="EnterOrExitCustomsTerritory" type="clt_ru:LetterIndicatorType">
            <xs:annotation>
              <xs:documentation>Тип декларации. 1 - Въезд, 2 - Выезд, 3 - Транзит</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeclarantPerson" type="pcd_bd:PersonType">
            <xs:annotation>
              <xs:documentation>Сведения о декларанте (физ.лицо)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CountryDeparture" type="cat_pcd:CountryType">
            <xs:annotation>
              <xs:documentation>Из какой страны прибыл (указывается страна отправления)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CountryDestination" type="cat_pcd:CountryType">
            <xs:annotation>
              <xs:documentation>В какую страну следует (указывается страна назначения)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Children" type="pcd_bd:HaveAndQuantityType">
            <xs:annotation>
              <xs:documentation>Совместно следуют несовершеннолетние (не достигшие шестнадцатилетнего возраста) дети</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MovementGoodsAndTransportMeansInfo" type="pcd_bd:MovementGoodsAndTransportMeansInfoType">
            <xs:annotation>
              <xs:documentation>Сведения о способе перемещения товаров и транспортных средств, пункт 2 Основного формуляра</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MovementCurrencyValuesInfo" type="pcd_bd:MovementCurrencyValuesInfoType" minOccurs="0">
            <xs:annotation>
              <!-- Не заполняется в случае типа декларации 3 - транзитная -->
              <xs:documentation>Сведения о перемещаемых валютных ценностях, пункты 3.1, 3.2 Основного формуляра</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TransportMeans" type="pcd_bd:TransportMeansType">
            <xs:annotation>
              <xs:documentation>Транспортные средства, пункт 3.3 Основного формуляра</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Goods" type="pcd_bd:GoodsType">
            <xs:annotation>
              <xs:documentation>Сведения о товарах, пункты 3.4, 3.5, 4 Основного формуляра</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DocDate" type="clt_ru:DateCustType">
            <xs:annotation>
              <xs:documentation>Дата составления декларации</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LanguagePassDecl" type="clt_ru:LanguageCodeType">
            <xs:annotation>
              <xs:documentation>Код языка заполнения документа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Signer" type="cat_pcd:SignerType">
            <xs:annotation>
              <!-- Внимание! Если тип декларации 3 - Транзит, то подать и подписать может только декларант -->
              <xs:documentation>Подпись лица, заявляющего сведения</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReservationID" type="clt_ru:DocumentIDType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Идентификатор резервирования денежных средств для целей уплаты таможенных платежей</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CustomsCode" type="clt_ru:CustomsCodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код таможенного органа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CustomsMark" type="cat_pcd:CustomsMarkType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о должностном лице таможенного органа, проставившем штамп и служебные отметки</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PCDAdditionalInfo" type="pcd_bd:PCDAdditionalInfoType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дополнительный формуляр (Декларация наличных денег и/или денежных инструментов)</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1006230E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные типы</xs:documentation>
  </xs:annotation>
  <xs:simpleType name="DocumentKindType">
    <xs:annotation>
      <xs:documentation>Перечислимый тип документов, подтверждающих заявленные сведения (1 - документы, подтверждающие соблюдение ограничений, связанных с перемещением товаров через таможенную границу; 2 - документы, позволяющие идентифицировать несопровождаемый багаж; 3 - документы, подтверждающие право на льготы по уплате таможенных платежей; 4 - документы, подтверждающие применение мер обеспечения соблюдения таможенной процедуры таможенного транзита; 5 - Иные документы)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="clt_ru:LetterIndicatorType">
      <xs:enumeration value="1">
        <xs:annotation>
          <xs:documentation>документы, подтверждающие соблюдение ограничений, связанных с перемещением товаров через таможенную границу</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation>
          <xs:documentation>документы, позволяющие идентифицировать несопровождаемый багаж</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation>
          <xs:documentation>документы, подтверждающие право на льготы по уплате таможенных платежей</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="4">
        <xs:annotation>
          <xs:documentation>документы, подтверждающие применение мер обеспечения соблюдения таможенной процедуры таможенного транзита</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="5">
        <xs:annotation>
          <xs:documentation>Иные документы</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="IntendedUseMoneyKindType">
    <xs:annotation>
      <xs:documentation>Перечислимый тип cведений о предполагаемом использовании наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:restriction base="clt_ru:LetterIndicatorType">
      <xs:enumeration value="1">
        <xs:annotation>
          <xs:documentation>текущие расходы (приобретение товаров и услуг)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation>
          <xs:documentation>инвестиции, включая приобретение недвижимости</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation>
          <xs:documentation>безвозмездные трансферты в пользу физических лиц (материальная помощь и т.п.)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="4">
        <xs:annotation>
          <xs:documentation>безвозмездные трансферты в пользу юридических лиц (благотворительность, пожертвования и т.п.)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="5">
        <xs:annotation>
          <xs:documentation>прочее</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="IntendedUseMoneyType">
    <xs:annotation>
      <xs:documentation>Сведения о предполагаемом использовании наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IntendedUseMoneyKind" type="pcd_bd:IntendedUseMoneyKindType">
        <xs:annotation>
          <xs:documentation>Вариант предполагаемого использование наличных денежных средств и (или) денежных инструментов (1 - текущие расходы (приобретение товаров и услуг); 2 - инвестиции, включая приобретение недвижимости; 3 - безвозмездные трансферты в пользу физических лиц (материальная помощь и т.п.); 4 - безвозмездные трансферты в пользу юридических лиц (благотворительность, пожертвования и т.п.); 5 - прочее)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IntendedUseMoneyOther" type="clt_ru:FreeText500Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Указание предполагаемого использования в случае выбора варианта 5 (прочее) </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="HaveAndQuantityType">
    <xs:annotation>
      <xs:documentation>Признак наличия и количество, если есть</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="HaveSign" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Наличие (1 или true - да; 0 или false - нет)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Quantity" type="clt_ru:Quantity3Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество (если есть наличие)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GoodsType">
    <xs:annotation>
      <xs:documentation>Сведения о товарах, пункты 3.4, 3.5, 4 Основного формуляра</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="GoodsForbiddenHaveSign" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Наличие товаров, в отношении которых применяются запреты или ограничения (1 или true - да; 0 или false - нет), пункт 3.4 Основного формуляра</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsIndivisibleHaveSign" type="clt_ru:IndicatorType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наличие неделимых товаров весом свыше порогового значения, товаров общим весом свыше порогового значения и (или) общей таможенной стоимостью свыше порогового значения (соответствующие пороговые значения устанавливаются Решением Совета ЕАЭС) (1 или true - да; 0 или false - нет), пункт 3.5 Основного формуляра</xs:documentation>
        </xs:annotation>
        <!-- Не заполняется в случае типа декларации 3 - транзитная -->
      </xs:element>
      <xs:element name="PassDeclGoodsInfo" type="pcd_bd:PassDeclGoodsInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сведения о товарах, указанных в подпунктах 3.4, 3.5, товарах, подлежащих таможенному декларированию, и иных товарах по желанию физического лица</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MonetaryInstrumentsDetailsType">
    <xs:annotation>
      <xs:documentation>Денежные инструменты, за исключением дорожных чеков (векселя, чеки (банковские), ценные бумаги на предъявителя и иное)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="pcd_bd:RecForeignCurDetailsBaseType">
        <xs:sequence>
          <xs:element name="MonetaryInstrumentKind" type="clt_ru:DigitalIndicatorType">
            <xs:annotation>
              <xs:documentation>Тип инструмента: 0 - вексель; 1 - банковский чек; 2 - ценные бумаги на предъявителя; 3 - иное</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MonetaryInstrumentName" type="clt_ru:DocumentNameType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Наименование типа инструмента в случае типа инструмента 3 - иное</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="WhoReleased" type="clt_ru:DocumentNameType">
            <xs:annotation>
              <xs:documentation>Кем выпущен</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DateReleased" type="clt_ru:DateCustType">
            <xs:annotation>
              <xs:documentation>Дата выпуска</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MonetaryInstrumentNumber" type="clt_ru:DocumentNumberType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Идентификационный номер (при наличии)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Quantity" type="clt_ru:Quantity3Type">
            <xs:annotation>
              <xs:documentation>Количество</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="MovementCurrencyValuesInfoType">
    <xs:annotation>
      <xs:documentation>Сведения о перемещаемых валютных ценностях, пункты 3.1, 3.2 Основного формуляра</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="NotExcessLimitForeignCurSign" type="pcd_bd:NotExcessLimitForeignCurSignType">
        <xs:annotation>
          <xs:documentation>Наличие валюты государств-членов таможенного союза, иностранной валюты, дорожных чеков в сумме, не превышающей 10000 долларов США в эквиваленте (да или нет, перечень (если есть, заполняется по желанию)), пункт 3.1 Основного формуляра</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExcessLimitForeignCurSign" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Есть валюта государств-членов таможенного союза, иностранная валюта, дорожные чеки в сумме, превышающей 10000 долларов США в эквиваленте, векселя, чеки (банковские), ценные бумаги на предъявителя (1 или true - да; 0 или false - нет), пункт 3.2 основного формуляра</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MovementGoodsAndTransportMeansInfoType">
    <xs:annotation>
      <xs:documentation>Сведения о способе перемещения товаров и транспортных средств, пункт 2 Основного формуляра</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AccompaniedLuggage" type="pcd_bd:HaveAndQuantityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Есть сопровождаемый багаж (да или нет, количество мест (если есть)), пункт 2.1 Основного формуляра</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UnaccompaniedLuggage" type="pcd_bd:HaveAndQuantityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Есть несопровождаемый багаж (да или нет, количество мест (если есть)), пункт 2.2 Основного формуляра</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsDeliveredByCarrier" type="pcd_bd:HaveAndQuantityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Есть товары, доставляемые перевозчиком (да или нет, количество мест (если есть)), пункт 2.3 Основного формуляра</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="NotExcessLimitForeignCurSignType">
    <xs:annotation>
      <xs:documentation>Наличие валюты государств-членов таможенного союза, иностранной валюты, дорожных чеков в сумме, не превышающей 10000 долларов США в эквиваленте (да или нет, перечень (если есть, заполняется по желанию))</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="NotExcessLimitForeignCurSign" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Есть валюта государств-членов таможенного союза, иностранная валюта, дорожные чеки в сумме, не превышающей 10000 долларов США в эквиваленте (1 или true - да; 0 или false - нет)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DetailsNotExcessLimitForeignCur" type="pcd_bd:RecForeignCurDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Валюта государств-членов таможенного союза, иностранная валюта, дорожные чеки в сумме, не превышающей 10000 долларов США в эквиваленте (заполняется по желанию физического лица)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="OriginMoneyKindType">
    <xs:annotation>
      <xs:documentation>Перечислимый тип сведений о происхождении наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:restriction base="clt_ru:Count2Type">
      <xs:enumeration value="1">
        <xs:annotation>
          <xs:documentation>Заработная плата, доходы от предпринимательской деятельности физического лица</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation>
          <xs:documentation>дивиденды и другие доходы от участия в капитале предприятий</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation>
          <xs:documentation>доходы от реализации движимого и недвижимого имущества</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="4">
        <xs:annotation>
          <xs:documentation>безвозмездные трансферты, полученные от физических и юридических лиц (материальная помощь, гранты и т.п.)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="5">
        <xs:annotation>
          <xs:documentation>пенсия, стипендия, социальные пособия, алименты</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="6">
        <xs:annotation>
          <xs:documentation>доходы от аренды недвижимости и земельных участков</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="7">
        <xs:annotation>
          <xs:documentation>заемные средства</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="8">
        <xs:annotation>
          <xs:documentation>наследство</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="9">
        <xs:annotation>
          <xs:documentation>прочее</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="OriginMoneyType">
    <xs:annotation>
      <xs:documentation>Сведения о происхождении наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OriginMoneyKind" type="pcd_bd:OriginMoneyKindType">
        <xs:annotation>
          <xs:documentation>Вариант происхождения наличных денежных средств и (или) денежных инструментов (1 - заработная плата, доходы от предпринимательской деятельности физического лица; 2 - дивиденды и другие доходы от участия в капитале предприятий; 3 - доходы от реализации движимого и недвижимого имущества; 4 - безвозмездные трансферты, полученные от физических и юридических лиц (материальная помощь, гранты и т.п.); 5 - пенсия, стипендия, социальные пособия, алименты; 6 - доходы от аренды недвижимости и земельных участков; 7 - заемные средства; 8 - наследство; 9 - прочее)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OriginMoneyOther" type="clt_ru:FreeText500Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Указание происхождения в случае выбора варианта 9 (прочее) </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
