<?xml version="1.0" encoding="UTF-8"?>
<!-- Схемы ГИС ГМП -  аналог документа - SQ.05010 -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:CategoryCust="urn:customs.ru:Categories:3.0.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.21.0" xmlns:ggpi_req="urn:customs.ru:Information:SQDocuments:GisGmpPaymentImportRequest:5.21.0" targetNamespace="urn:customs.ru:Information:SQDocuments:GisGmpPaymentImportRequest:5.21.0" elementFormDefault="qualified" version="5.21.0.0">
  <xs:import namespace="urn:customs.ru:Categories:3.0.0" schemaLocation="CategoriesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.21.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Прием необходимой для уплаты информации (начисления) (запрос)</xs:documentation>
  </xs:annotation>
  <xs:element name="GisGmpPaymentImportRequest" type="ggpi_req:GisGmpPaymentImportRequestType">
    <xs:annotation>
      <xs:documentation>Прием необходимой для уплаты информации (начисления) (запрос)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="GisGmpPaymentImportRequestType">
    <xs:annotation>
      <xs:documentation>Прием необходимой для уплаты информации (начисления) (запрос)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:choice>
            <xs:element name="ChargeTemplate" type="ggpi_req:ChargeTemplateType" maxOccurs="100">
              <xs:annotation>
                <xs:documentation>Направляемое начисление</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangeTemplate" type="ggpi_req:ChangeTemplateType" maxOccurs="100">
              <xs:annotation>
                <xs:documentation>Направляемые изменения в извещение о начислении</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1009173E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные типы</xs:documentation>
  </xs:annotation>
  <xs:complexType name="AdditionalDataType">
    <xs:annotation>
      <xs:documentation>Дополнительные поля начисления</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="ggpi_req:TextVariable100Type">
        <xs:annotation>
          <xs:documentation>Наименование поля</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Значение поля</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalOffenseType">
    <xs:annotation>
      <xs:documentation>Блок дополнительной информации об административном правонарушении</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OffenseDate" type="clt_ru:DateTimeCustType">
        <xs:annotation>
          <xs:documentation>Дата и время нарушения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OffensePlace" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Место нарушения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LegalAct" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Статья нарушения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DigitalLink" type="ggpi_req:TextVariable2000Type">
        <xs:annotation>
          <xs:documentation>Ссылка на фото (видео) материалов нарушения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DepartmentName" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Данные о подразделении, вынесшем постановление</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BankType">
    <xs:annotation>
      <xs:documentation>Данные банка, в котором открыт счет</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="ggpi_req:TextVariable200Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование структурного подразделения кредитной организации или подразделения Банка России, в котором открыт счет</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BIK" type="clt_ru:BICIDType">
        <xs:annotation>
          <xs:documentation>БИК структурного подразделения кредитной организации или подразделения Банка России, в котором открыт счет</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CorrespondentBankAccount" type="clt_ru:AccountType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер корреспондентского счета кредитной организации, открытый в подразделении Банка России</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BudgetIndexType">
    <xs:annotation>
      <xs:documentation>Реквизиты платежа 101, 106-109</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Status" type="clt_ru:Code2Type">
        <xs:annotation>
          <xs:documentation>Статус плательщика — реквизит 101 Распоряжения.
                        До 01.07.2021 реквизит платежа 101 «Статус плательщика» блока данных «Дополнительные реквизиты платежа» может принимать значение из диапазонов "01"-"13", "15"-"28".
                        C 01.07.2021 реквизит платежа 101 «Статус плательщика» блока данных «Дополнительные реквизиты платежа» может принимать значение из диапазонов "01"-"13", "15-29".
                        С 01.10.2021 реквизит платежа 101 «Статус плательщика» блока данных «Дополнительные реквизиты платежа» может принимать значение из диапазонов "01"-"08", "13","15"-"17", "19"-"20", "23"-"24", "27"-"30".
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaytReason" type="clt_ru:CodeVariable2Type">
        <xs:annotation>
          <xs:documentation>Показатель основания платежа — реквизит 106 Распоряжения.
                        С 01.01.2021 реквизит 106 «Показатель основания платежа» блока данных «Дополнительные реквизиты платежа» может содержать значение "0" или одно из следующих значений: ТП, ЗД, БФ, ТР, РС, ОТ, РТ, ПБ, ПР, АП, АР, ИН, ТЛ, БД, ПК, КВ, УВ, ИЛ, ТГ, ТБ, ТД, ПВ, ЗТ, 00.
                        С 01.10.2021 реквизит 106 «Показатель основания платежа» блока данных «Дополнительные реквизиты платежа» может содержать значение "0" или одно из следующих значений: ТП, ЗД, РС, ОТ, РТ, ПБ, ИН, ТЛ, ЗТ, ПК, КЭ, УВ, ИЛ, ТГ, ТБ, ТД, ПВ, 00.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxPeriod" type="ggpi_req:TaxPeriodType">
        <xs:annotation>
          <xs:documentation>
                        Показатель налогового периода или код таможенного органа, осуществляющего в соответствии с
                        законодательством РФ функции по выработке государственной политики и нормативному регулированию,
                        контролю и надзору в области таможенного дела – реквизит 107 Распоряжения.
                        Если длина значения в поле 10 символов, то:
                        символы 1-й и 2-й могут принимать значение: МС, КВ, ПЛ, ГД;
                        символы 4-й и 5-й могут принимать значение: для месячных платежей ‑ номер месяца текущего отчетного года, для квартальных платежей - номер квартала, для полугодовых - номер полугодия;
                        символы с 7-го по 10-й могут принимать значение: год, за который производится уплата налога;
                        символы 3-й и 6-й используются в качестве разделительных знаков, в них проставляется точка (".").
                        Если длина поля 8 символов, то все они должны быть цифрами.
                        Допускается указывать дату в формате "день.месяц.год".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxDocNumber" type="ggpi_req:TextVariable15Type">
        <xs:annotation>
          <xs:documentation>Показатель номера документа – реквизит 108 Распоряжения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxDocDate" type="ggpi_req:TaxDocDateType">
        <xs:annotation>
          <xs:documentation>Показатель даты документа – реквизит 109 Распоряжения. Особенности заполнения:
                        первые два символа обозначают календарный день (могут принимать значения от 01 до 31);
                        4-й и 5-й символы - месяц (значения от 01 до 12);
                        с 7-го по 10-й   год;
                        в 3-м и 6-м символах в качестве разделительных знаков проставляется точка (".");
                        Значение «00» может быть указано в случае указания значения «00» в поле номер 106: Показатель основания платежа — реквизит 106 Распоряжения.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChangeStatusType">
    <xs:annotation>
      <xs:documentation>Сведения о статусе и основаниях его изменения</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Meaning" type="ggpi_req:MeaningType">
        <xs:annotation>
          <xs:documentation>Статус, отражающий изменение данных: 2 – уточнение; 3 – уточнение об аннулировании; 4 – уточнение о деаннулировании (отмена аннулирования).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reason" type="clt_ru:FreeText500Type">
        <xs:annotation>
          <xs:documentation>Основание изменения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChangeDate" type="clt_ru:DateTimeCustType">
        <xs:annotation>
          <xs:documentation>Дата, а также сведения о периоде времени, в который осуществлено уточнение необходимой для уплаты информации (до 21 часа или после 21 часа по местному времени), либо время уточнения необходимой для уплаты информации. При указании сведений о периоде времени, в который осуществлено уточнение необходимой для уплаты информации, для обозначения периода времени до 21 часа по местному времени используется значение «20:59:59», после 21 часа по местному времени – «21:01:00»</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChangeTemplateType">
    <xs:annotation>
      <xs:documentation>Направляемые изменения в извещение о начислении</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OriginatorID" type="ggpi_req:URNType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>УРН участника косвенного взаимодействия, сформировавшего сущность</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SupplierBillID" type="ggpi_req:SupplierBillIDType">
        <xs:annotation>
          <xs:documentation>УИН</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Change" type="ggpi_req:ChangeType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Измененяемые поля</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChangeStatus" type="ggpi_req:ChangeStatusType">
        <xs:annotation>
          <xs:documentation>Сведения о статусе и основаниях его изменения</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChangeType">
    <xs:annotation>
      <xs:documentation>Измененяемые поля</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FieldNum" type="ggpi_req:FieldNumType">
        <xs:annotation>
          <xs:documentation>Номер поля, в которое вносятся изменения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChangeValue" type="ggpi_req:ChangeValueType" maxOccurs="10">
        <xs:annotation>
          <xs:documentation>Новое значение поля</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChangeValueType">
    <xs:annotation>
      <xs:documentation>Новое значение поля</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="ggpi_req:TextVariable100Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование дополнительного поля. Используется только для отдельных контейнеров -AdditionalData, (поле номер 202), DiscountFixed (поле номер 1020), DiscountSize (поле номер 1021), MultiplierSize (поле номер 1022)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Значение изменяемого поля. Если требуется не изменить, а удалить переданное ранее значение поля, то в поле следует указать значение NULL.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChargeTemplateType">
    <xs:annotation>
      <xs:documentation>Данные для формирования необходимой для уплаты информации</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OriginatorID" type="ggpi_req:URNType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>УРН участника косвенного взаимодействия, сформировавшего запрос</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SupplierBillID" type="ggpi_req:SupplierBillIDType">
        <xs:annotation>
          <xs:documentation>УИН</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BillDate" type="clt_ru:DateTimeCustType">
        <xs:annotation>
          <xs:documentation>Дата начисления суммы, подлежащей уплате плательщиком</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValidUntil" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата, до которой (включительно) актуально выставленное начисление</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalAmount" type="ggpi_req:LongIntegerType">
        <xs:annotation>
          <xs:documentation>Сумма начисления (Целое число, показывающее сумму в копейках)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Purpose" type="ggpi_req:PurposeType">
        <xs:annotation>
          <xs:documentation>Назначение платежа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="KBK" type="ggpi_req:KBKType">
        <xs:annotation>
          <xs:documentation>КБК. В случае отсутствия следует указывать значение «0». Все символы одновременно не должны принимать значение ноль («0»)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OKTMO" type="ggpi_req:OKTMOType">
        <xs:annotation>
          <xs:documentation>Код ОКТМО. Значение «0» в случае отсутствия кода ОКТМО может указываться, если первые цифры номера банковского счета получателя средств (AccountNumber) равны «03100» и КБК начинается на «153» или если первые цифры номера банковского счета получателя средств (AccountNumber) не равны «03100»</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DeliveryDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата отсылки (вручения) плательщику документа с начислением в случае, если этот документ был отослан (вручен) получателем средств плательщику
                        (Заполняется в случае, если документ был вручен плательщику получателем средств)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LegalAct" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Информация о нормативном правовом (правовом) акте, являющемся основанием для исчисления суммы денежных средств, подлежащих уплате</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaymentTerm" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Срок оплаты начисления</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Origin" type="ggpi_req:OriginType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Признак предварительного начисления. Возможные значения:
                        PRIOR – признак указывается участником в начислениях при направлении дела на рассмотрение в суд и  в начислениях, сформированных ФССП России. При указании данного признака, в начислении возможно указать только нулевую сумму.
                        TEMP – для предварительных начислений, сформированных ГИС ГМП по запросу участника и имеющих срок действия. Допускается указывать для извещений со статусом, отражающим изменение данных: 2 – уточнение; 3 – уточнение об аннулировании; 4 – уточнение о деаннулировании.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NoticeTerm" type="CategoryCust:IntegerCategoriesCust" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество дней от даты начисления, подлежащей уплате плательщиком, по истечении которых необходимо повторно предоставлять уведомление о начислении по подписке в случае, если оно не оплачено или сумма платежей меньше суммы к оплате, указанной в начислении</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OKVED" type="ggpi_req:OkvedType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код по ОКВЭД</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChargeOffense" type="clt_ru:IndicatorType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Признак административного правонарушения, зафиксированного специальными техническими средствами, работающими в автоматическом режиме</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LinkedChargesIdentifiers" type="ggpi_req:LinkedChargesIdentifiersType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Идентификаторы начислений, на основании которых выставлено данное начисление. Может быть указано до 10 УИН</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Payee" type="ggpi_req:PayeeType">
        <xs:annotation>
          <xs:documentation>Данные организации, являющейся получателем средств</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Payer" type="ggpi_req:PayerType">
        <xs:annotation>
          <xs:documentation>Сведения о плательщике</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BudgetIndex" type="ggpi_req:BudgetIndexType">
        <xs:annotation>
          <xs:documentation>Реквизиты платежа 101, 106 - 109</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExecutiveProcedureInfo" type="ggpi_req:ExecutiveProcedureInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дополнительная информация, необходимая для осуществления исполнительного производства</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AdditionalOffense" type="ggpi_req:AdditionalOffenseType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Блок дополнительной информации об административном правонарушении</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Discount" type="ggpi_req:DiscountType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дополнительные условия оплаты</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AdditionalData" type="ggpi_req:AdditionalDataType" minOccurs="0" maxOccurs="10">
        <xs:annotation>
          <xs:documentation>Дополнительные поля начисления</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
