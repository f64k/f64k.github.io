<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.21.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:enrq="urn:customs.ru:Information:SQDocuments:ExportNoticeRequest:5.21.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:CategoryCust="urn:customs.ru:Categories:3.0.0" targetNamespace="urn:customs.ru:Information:SQDocuments:ExportNoticeRequest:5.21.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.21.0.0">
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.21.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Categories:3.0.0" schemaLocation="CategoriesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Запрос на предоставление из ГИС ГМП уведомлений по подписке</xs:documentation>
  </xs:annotation>
  <xs:element name="ExportNoticeRequest" type="enrq:ExportNoticeRequestType">
    <xs:annotation>
      <xs:documentation>Запрос на предоставление из ГИС ГМП уведомлений по подписке</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="ExportNoticeRequestType">
    <xs:annotation>
      <xs:documentation>Запрос на предоставление из ГИС ГМП уведомлений по подписке</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="Id" type="clt_ru:Text50Type">
            <xs:annotation>
              <xs:documentation>Идентификатор запроса</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Timestamp" type="clt_ru:DateTimeCustType">
            <xs:annotation>
              <xs:documentation>Дата и время формирования сообщения</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Destination" type="enrq:DestinationType">
            <xs:annotation>
              <xs:documentation>Идентификаторы получателя уведомлений по подписке</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element name="NoticeCharge" type="enrq:NoticeChargeType">
              <xs:annotation>
                <xs:documentation>Уведомления о начислении, уточнении или аннулировании начисления</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="NoticePayment" type="enrq:NoticePaymentType">
              <xs:annotation>
                <xs:documentation>Уведомления о поступившем платеже, уточнении или аннулировании платежа</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="NoticeQuittance" type="enrq:NoticeQuittanceType">
              <xs:annotation>
                <xs:documentation>Уведомления о формировании квитанции</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="NoticeRenouncement" type="enrq:NoticeRenouncementType">
              <xs:annotation>
                <xs:documentation>Уведомление о поступившем извещении об отказе в возбуждении исполнительного производства
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="NoticeChargeExecutive" type="enrq:NoticeChargeExecutiveType">
              <xs:annotation>
                <xs:documentation>Уведомления о неуплате начисления в установленный законодательством срок</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1009332E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные типы</xs:documentation>
  </xs:annotation>
  <xs:complexType name="AccDocType">
    <xs:annotation>
      <xs:documentation>Сведения о документе</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AccDocNo" type="enrq:AccDocNoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Поле номер 3. Для частичного платежа поле номер 40:
                        Номер платежного документа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AccDocDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Поле номер 4. Для частичного платежа поле номер 41:
                        Дата платежного документа</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AccPayerType">
    <xs:annotation>
      <xs:documentation>Сведения о плательщике в организации, принявшей платеж  </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PayerAccount" type="clt_ru:AccountType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Поле номер 9:
                        Номер счета плательщика (при наличии) в организации, принявшей платеж  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayerIdentifier" type="enrq:PayerIdentifierType">
        <xs:annotation>
          <xs:documentation>Идентификатор плательщика</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayerName" type="enrq:PayerNameType">
        <xs:annotation>
          <xs:documentation>Поле номер 8: Плательщик</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalDataType">
    <xs:annotation>
      <xs:documentation>Дополнительные сведения</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="enrq:String100Type">
        <xs:annotation>
          <xs:documentation>Наименование поля</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" type="enrq:String255Type">
        <xs:annotation>
          <xs:documentation>Значение поля</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApproverType">
    <xs:annotation>
      <xs:documentation>Информация об утверждении постановления об отказе в возбуждении ИП</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PositionCode" type="enrq:String25Type">
        <xs:annotation>
          <xs:documentation>Код должности СПИ/должностного лица ФССП России, утвердившего документ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PositionName" type="enrq:String255Type">
        <xs:annotation>
          <xs:documentation>Наименование должности лица, утвердившего документ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PersonApproving" type="enrq:String1000Type">
        <xs:annotation>
          <xs:documentation>ФИО лица, утвердившего документ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApprovalDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Дата утверждения</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BankType">
    <xs:annotation>
      <xs:documentation>Данные ТОФК, структурных подразделений кредитных организаций или подразделений Банка России</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CorrespondentBankAccount" type="clt_ru:AccountType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Поле номер 15 для получателя средств. Поле номер 12 для организации, принявшей платеж.
                        Номер единого казначейского счета или корреспондентского счета кредитной организации, открытый в подразделении Банка России.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" type="enrq:String200Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Поле номер 13 для получателя средств. Поле номер 10 для организации, принявшей платеж.
                        Наименование ТОФК, структурного подразделения кредитной организации или подразделения Банка России, в котором открыт счет.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BIK" type="clt_ru:BICIDType">
        <xs:annotation>
          <xs:documentation>Поле номер 14 для получателя средств. Поле номер 11 для организации, принявшей платеж.      БИК ТОФК, структурного подразделения кредитной организации или подразделения Банка России, в котором открыт счет</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BudgetIndexType">
    <xs:annotation>
      <xs:documentation>Сведения о платеже</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Status" type="clt_ru:CodeVariable2Type">
        <xs:annotation>
          <xs:documentation>Поле номер 101: Статус плательщика - реквизит 101 Распоряжения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaytReason" type="clt_ru:CodeVariable2Type">
        <xs:annotation>
          <xs:documentation>Поле номер 106: Показатель основания платежа - реквизит 106 Распоряжения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxDocDate" type="enrq:TaxDocDateType">
        <xs:annotation>
          <xs:documentation>Поле номер 109: Показатель даты документа - реквизит 109 Распоряжения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxDocNumber" type="enrq:TaxDocNumberType">
        <xs:annotation>
          <xs:documentation>Поле номер 108: Показатель номера документа - реквизит 108 Распоряжения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxPeriod" type="enrq:TaxPeriodType">
        <xs:annotation>
          <xs:documentation>Поле номер 107:
                        Показатель налогового периода или код таможенного органа, осуществляющего в соответствии с законодательством РФ функции по выработке государственной политики и нормативному регулированию, контролю и надзору в области таможенного дела – реквизит 107 Распоряжения.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChangeStatusInfoType">
    <xs:annotation>
      <xs:documentation>Сведения об изменении статуса</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Meaning" type="enrq:MeaningType">
        <xs:annotation>
          <xs:documentation>Статус, отражающий изменение данных:
                        1 - новый
                        2 - уточнение
                        3 - аннулирование
                        4 - деаннулировании</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reason" type="enrq:ReasonType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Основание изменения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChangeDate" type="clt_ru:DateTimeCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата и время уточнения информации</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClarificationType">
    <xs:annotation>
      <xs:documentation>Сведения об уточнениях вида и принадлежности платежа</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ClarificationId" type="enrq:ClarificationIdType">
        <xs:annotation>
          <xs:documentation>Уникальный идентификатор уточнения вида и принадлежности платежа (УВПП)
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DebtorType">
    <xs:annotation>
      <xs:documentation>Сведения о должнике</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DebtorType" type="clt_ru:CodeVariable4Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1121: Тип должника</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DebtorAdr" type="enrq:String200Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1122: Адрес должника</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DebtorAdrFakt" type="enrq:String200Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Поле номер 1123: Адрес должника фактический</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DebtorCountryCode" type="clt_ru:Code3Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Поле номер 1124: Код страны принадлежности должника по Общероссийскому
                        классификатору стран мира
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Person" type="enrq:PersonType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Данные индивидуального предпринимателя или физического лица</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DeedInfoType">
    <xs:annotation>
      <xs:documentation>Данные исполнительного документа</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IDType" type="clt_ru:CodeVariable2Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1109: Код вида исполнительного документа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdDocNo" type="enrq:String25Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1110: Номер исполнительного документа, присвоенный органом, выдавшим
                        исполнительный документ
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdDocDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Поле номер 1111: Дата выдачи исполнительного документа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SubjCode" type="enrq:Code7Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1112: Код предмета исполнения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SubjName" type="enrq:String1000Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1113: Предмет исполнения</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DestinationType">
    <xs:annotation>
      <xs:documentation>Идентификаторы получателя уведомлений по подписке</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RecipientIdentifier" type="enrq:URNType">
        <xs:annotation>
          <xs:documentation>УРН участника получателя</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RoutingCode" type="enrq:RoutingCodeType">
        <xs:annotation>
          <xs:documentation>Код маршрутизации участника для предоставления информации по ВС с табличной маршрутизацией</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DiscountType">
    <xs:annotation>
      <xs:documentation>Сведения о дополнительных условиях оплаты</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Value" type="enrq:DiscountValueType">
        <xs:annotation>
          <xs:documentation>Значение</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Expiry" type="enrq:DiscountDateType">
        <xs:annotation>
          <xs:documentation>Срок действия</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DiscountValueType">
    <xs:annotation>
      <xs:documentation>Значение/процент/коэффициент </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="FloatValue" type="CategoryCust:NumericCategoriesCust">
          <xs:annotation>
            <xs:documentation>Число с плавающей точкой</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="NonNegativeIntegerValue" type="enrq:NonNegativeIntegerValueType">
          <xs:annotation>
            <xs:documentation>Целое положительное число</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ExecutiveProcedureInfoType">
    <xs:annotation>
      <xs:documentation>Информация, необходимая для осуществления исполнительного производства</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IdDeloNo" type="enrq:String25Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1101: Номер дела или материалов</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DeloPlace" type="clt_ru:Text150Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Поле номер 1102: Место рассмотрения дела</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdDesDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Поле номер 1103: Дата принятия решения по делу</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AktDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Поле номер 1104: Дата вступления решения в законную силу</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SrokPrIsp" type="enrq:LongType">
        <xs:annotation>
          <xs:documentation>Поле номер 1105: Срок предъявления исполнительного документа к исполнению
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SrokPrIspType" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Поле номер 1106: Тип периода срока предъявления исполнительного документа
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClaimerAdr" type="enrq:String200Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1107: Адрес взыскателя</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NotifFSSPDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Поле номер 1108: Дата уведомления ФССП России о неуплате штрафа в установленный
                        законом
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DeedInfo" type="enrq:DeedInfoType">
        <xs:annotation>
          <xs:documentation>Данные исполнительного документа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExecutOrgan" type="enrq:ExecutOrganType">
        <xs:annotation>
          <xs:documentation>Данные органа, выдавшего исполнительный документ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Debtor" type="enrq:DebtorType">
        <xs:annotation>
          <xs:documentation>Сведения о должнике</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ExecutOrganType">
    <xs:annotation>
      <xs:documentation>Данные органа, выдавшего исполнительный документ</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OrganOkogu" type="enrq:Code7Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1114: Код по Общероссийскому классификатору органов государственной
                        власти и управления (ОКОГУ) органа, выдавшего исполнительный документ
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganCode" type="enrq:String10Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1115: Код подразделения органа, выдавшего исполнительный документ
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Organ" type="enrq:String1000Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1116: Наименование подразделения уполномоченного органа,
                        выдавшего исполнительный документ
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganAdr" type="enrq:String200Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1117: Адрес органа, выдавшего исполнительный документ
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganSignCodePost" type="enrq:String25Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1118: Код должности лица, выдавшего исполнительный документ
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganSign" type="enrq:String255Type">
        <xs:annotation>
          <xs:documentation>Поле номер 1119: Должность лица, выдавшего исполнительный документ
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
