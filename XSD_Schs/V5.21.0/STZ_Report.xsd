              <xs:documentation>Отчет о товарах, помещенных под  таможенную процедуру СТЗ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NonRezidentTable" type="stz:NonRezidentTableType" minOccurs="0">
