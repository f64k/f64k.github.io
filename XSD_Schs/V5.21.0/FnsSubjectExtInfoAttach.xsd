          <xs:documentation>Дата возбуждения дела</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxOrganCode" type="clt_ru:TaxOrganIDType">
        <xs:annotation>
          <xs:documentation>Код налогового органа, возбудившего дело</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reason" type="clt_ru:FreeText500Type" maxOccurs="2">
        <xs:annotation>
          <xs:documentation>Основание возбуждения дела</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ArticleKOAP" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Статья КОАП, по которой возбуждено административное дело</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DateSendCaseCourt" type="clt_ru:DateCustType" minOccurs="0">
