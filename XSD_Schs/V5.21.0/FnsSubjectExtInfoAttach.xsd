          <xs:documentation>Номер решения по делу</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PunishmentKind" type="clt_ru:FreeText500Type" minOccurs="0" maxOccurs="2">
        <xs:annotation>
          <xs:documentation>Вид назначенного наказания</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AmountPenalty" type="clt_ru:AmountType" minOccurs="0">
