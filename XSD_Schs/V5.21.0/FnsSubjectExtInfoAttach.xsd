      </xs:element>
      <xs:element name="DateDefinition" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата определения о возбуждении дела об административном правонарушении и проведении административного расследования</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumDefinitionRefusal" type="clt_ru:FreeText250Type" minOccurs="0">
