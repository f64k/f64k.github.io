    </xs:annotation>
    <xs:sequence>
      <xs:element name="Jurisdiction" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Юрисдикция (1 – российской лицо; 2 – иностранное лицо)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PersonKind" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Вид лица (1 – юридическое лицо; 2 – физическое лицо или индивидуальный предприниматель)</xs:documentation>
        </xs:annotation>
