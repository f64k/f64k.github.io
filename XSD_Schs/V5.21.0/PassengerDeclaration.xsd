<?xml version="1.0" encoding="UTF-8"?>
<!--  Решение Коллегии Евразийской экономической комиссии от 12.05.2020 N 63 "О структуре и формате пассажирской таможенной декларации" -->
<!--  Решение Коллегии Евразийской экономической комиссии от 23 июля 2019 г. N 124 "О таможенном декларировании товаров для личного пользования" -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.21.0" xmlns:catESAD_cu="urn:customs.ru:CUESADCommonAggregateTypesCust:5.21.0" xmlns:pas_d="urn:customs.ru:Information:CustomsDocuments:PassengerDeclaration:5.21.0" xmlns:RUScat_ru="urn:customs.ru:RUSCommonAggregateTypes:5.21.0" xmlns:RUSclt_ru="urn:customs.ru:RUSCommonLeafTypes:5.21.0" xmlns:RUDECLcat="urn:customs.ru:RUDeclCommonAggregateTypesCust:5.21.0" targetNamespace="urn:customs.ru:Information:CustomsDocuments:PassengerDeclaration:5.21.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.21.0.0">
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.21.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.21.0" schemaLocation="CUESADCommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:RUSCommonAggregateTypes:5.21.0" schemaLocation="RUSCommonAggregateTypes.xsd" />
  <xs:import namespace="urn:customs.ru:RUSCommonLeafTypes:5.21.0" schemaLocation="RUSCommonLeafTypes.xsd" />
  <xs:import namespace="urn:customs.ru:RUDeclCommonAggregateTypesCust:5.21.0" schemaLocation="RUDeclCommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Пассажирская таможенная декларация</xs:documentation>
  </xs:annotation>
  <xs:element name="PassengerDeclaration" type="pas_d:PassengerDeclarationType">
    <xs:annotation>
      <xs:documentation>Пассажирская таможенная декларация</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="PassengerDeclarationType">
    <xs:annotation>
      <xs:documentation>Пассажирская таможенная декларация</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="EECEDocHeaderAddInfo" type="RUScat_ru:EECEDocHeaderAddInfoType">
            <xs:annotation>
              <xs:documentation>Дополнительные технологические реквизиты электронного документа (сведений)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MovingCode" type="clt_ru:LetterIndicatorType">
            <xs:annotation>
              <xs:documentation>Кодовое обозначение способа перемещения:                              1 – сопровождаемый багаж, включая ручную кладь;
2 – несопровождаемый багаж (багаж, следующий отдельно от въезжающего/выезжающего лица); 3 – доставляемые в адрес (пересылаемые) товары без въезда/выезда лица</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ElectronicDocumentSign" type="clt_ru:Code2Type">
            <xs:annotation>
              <xs:documentation>Признак использования ПТД в форме электронного документа. ЭД</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeclarantPerson" type="pas_d:PersonType">
            <xs:annotation>
              <xs:documentation>Сведения о декларанте</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MinorPersonQuantity" type="clt_ru:Quantity4Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Количество лиц, не достигших 16-летнего возраста</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GoodsShipmentDetails" type="pas_d:GoodsShipmentDetailsType">
            <xs:annotation>
              <xs:documentation>Сведения о товарах и транспортных средствах для личного пользования</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RoleCode" type="clt_ru:LetterIndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код роли лица, заполнившего документ                                      1 – декларант;
2 – работник таможенного представителя;
3 – лицо, действующее от имени и по поручению декларанта </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SignatoryRepresentativeDetails" type="RUDECLcat:SignatoryRepresentativeDetailsType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о таможенном представителе, заполнившем (подписавшем) таможенный документ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FilledPerson" type="RUDECLcat:SignatoryPersonDetailsType">
            <xs:annotation>
              <xs:documentation>Сведения о физическом лице, заполнившем (подписавшем) таможенный документ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CorrectionDetails" type="pas_d:CorrectionDetailsType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения, указываемые при внесении изменений (дополнений) в пассажирскую таможенную декларацию после выпуска товаров</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReservationID" type="clt_ru:DocumentIDType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Идентификатор резервирования денежных средств для целей уплаты таможенных платежей</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TransportModeCode" type="clt_ru:TransportModeCodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код вида транспортного средства, на котором прибыл пассажир</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1006300E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные типы</xs:documentation>
  </xs:annotation>
  <xs:complexType name="CorrectionDetailsType">
    <xs:annotation>
      <xs:documentation>Сведения, указываемые  при внесении изменений (дополнений) в пассажирскую таможенную декларацию после выпуска товаров</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PTDRegNum" type="cat_ru:GTDIDType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Регистрационный номер пассажирской таможенной декларации, в которую вносятся изменения (дополнения)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Description" type="RUSclt_ru:FreeText4000Type" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Обоснование необходимости внесения изменений (дополнений) в пассажирскую таможенную декларацию</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EngineType">
    <xs:annotation>
      <xs:documentation>Сведения о двигателе</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="EngineModeCode" type="clt_ru:CodeVariable2Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код типа двигателя: 1 - бензиновый, 2 - дизельный, 3 - электрический, 4 - гибридный на основе бензинового, 5 - гибридный на основе дизельного, 6 - конв. для работы на газе, 7 - иной.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EngineModeName" type="clt_ru:Text50Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование типа двигателя</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EngineModel" type="clt_ru:Text50Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Модель двигателя</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FuelName" type="clt_ru:Text50Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Тип топлива</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EnginePowerHpQuanity" type="clt_ru:EnginePowerQuanityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Мощность двигателя в лошадиных силах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EnginePowerKvtQuanity" type="clt_ru:EnginePowerQuanityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Мощность двигателя в киловаттах</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GoodsLitsDetailsType">
    <xs:annotation>
      <xs:documentation>Дополнительные сведения о товарах</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PassDeclGoods" type="pas_d:PassDeclGoodsType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения о товаре</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PersonName" type="pas_d:PersonNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>ФИО лица, не достигшего 16-летнего возраста, совместно следующего с декларантом</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdentityDoc" type="RUScat_ru:RUIdentityCardType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Cведения о документе, удостоверяющем личность лица, не достигшего 16-летнего возраста, совместно следующего с декларантом</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GoodsShipmentDetailsType">
    <xs:annotation>
      <xs:documentation>Сведения о товарах и транспортных средствах для личного пользования</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PassDeclGoodsInfo" type="pas_d:PassDeclGoodsInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сведения о товарах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransportMeans" type="pas_d:TransportMeansType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сведения о транспортных средствах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MovementCurrencyValuesInfo" type="pas_d:MovementCurrencyValuesInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сведения о наличных денежных средствах и (или) денежных инструментах</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="IntendedUseMoneyType">
    <xs:annotation>
      <xs:documentation>Сведения о предполагаемом использовании наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IntendedUseMoneyKind" type="clt_ru:Code2Type" maxOccurs="5">
        <xs:annotation>
          <xs:documentation>Кодовое обозначение цели предполагаемого использования денежных средств и (или) денежных инструментов:                                     01 – текущие расходы (приобретение товаров и услуг);
02 – инвестиции, включая приобретение недвижимости;
03 – безвозмездные трансферты в пользу физических лиц (материальная помощь и т. п.);
04 – безвозмездные трансферты в пользу юридических лиц (благотворительность, пожертвования и т. п.);
99 – прочие цели использования
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IntendedUseMoneyOther" type="RUSclt_ru:FreeText4000Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Описание прочих целей предполагаемого использования наличных денежных средств и (или) денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ItineraryPointDetailsType">
    <xs:annotation>
      <xs:documentation>Сведения о пункте маршрута перевозки наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CountryCode" type="clt_ru:CountryA2CodeType">
        <xs:annotation>
          <xs:documentation>Код страны</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PlaceName" type="clt_ru:ShortNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование (название) места (географического пункта)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ObjectOrdinal" type="clt_ru:Count3Type">
        <xs:annotation>
          <xs:documentation>Порядковый номер маршрута</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Customs" type="cat_ru:CustomsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Таможенный орган</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Date" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MonetaryInstrumentsDetailsType">
    <xs:annotation>
      <xs:documentation>Денежные инструменты, за исключением дорожных чеков (векселя, чеки (банковские), ценные бумаги на предъявителя и иное)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MonetaryInstrumentName" type="clt_ru:NameType">
        <xs:annotation>
          <xs:documentation>Наименование денежного инструмента</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Issuer" type="RUSclt_ru:Name300Type">
        <xs:annotation>
          <xs:documentation>Эмитент</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DateReleased" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Дата выпуска</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="RUScat_ru:AmountType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номинальная стоимость или сумма</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NegotiableInstrumentId" type="clt_ru:Text40Type" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Идентифицирующий номер денежного инструмента</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Quantity" type="clt_ru:Quantity4Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MovementCurrencyValuesInfoType">
    <xs:annotation>
      <xs:documentation>Сведения о наличных денежных средствах и (или) денежных инструментах</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CashDetails" type="RUScat_ru:AmountType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения о наличных денежных средствах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TravelersChequeDetails" type="RUScat_ru:AmountType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения о дорожных чеках</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MonetaryInstrumentsDetails" type="pas_d:MonetaryInstrumentsDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения о денежных инструментах, за исключением дорожных чеков </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerMoney" type="pas_d:OwnerMoneyType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения о владельце наличных денежных средств и (или) денежных инструментов – указывается, если декларант не является собственником</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OriginMoney" type="pas_d:OriginMoneyType">
        <xs:annotation>
          <xs:documentation>Сведения о происхождении наличных денежных средств и (или) денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IntendedUseMoney" type="pas_d:IntendedUseMoneyType">
        <xs:annotation>
          <xs:documentation>Сведения о предполагаемом использовании наличных денежных средств и (или) денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RouteMoney" type="pas_d:RouteMoneyType">
        <xs:annotation>
          <xs:documentation>Сведения о маршруте и способе перевозки наличных денежных средств и (или) денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OriginMoneyType">
    <xs:annotation>
      <xs:documentation>Сведения о происхождении наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OriginMoneyKind" type="clt_ru:Code2Type" maxOccurs="9">
        <xs:annotation>
          <xs:documentation>Кодовое обозначение источника происхождения наличных денежных средств и (или) денежных инструментов          01 – заработная плата, доходы от предпринимательской деятельности физического лица;
02 – дивиденды и другие доходы от участия в уставном (акционерном и т. п.) капитале организаций;
03 – доходы от реализации движимого и (или) недвижимого имущества;
04 – безвозмездные трансферты, полученные от физических и (или) юридических лиц (материальная помощь, гранты и т. п.);
05 – пенсия, стипендия, социальные пособия, алименты;
06 – доходы от аренды недвижимости и земельных участков;
07 – заемные средства;
08 – наследство;
99 – прочие источники происхождения
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OriginMoneyOther" type="RUSclt_ru:FreeText4000Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Описание прочих источников происхождения денежных средств и (или) денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OwnerMoneyType">
    <xs:annotation>
      <xs:documentation>Сведения о владельце наличных денежных средств и (или) денежных инструментов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OwnerMoneyName" type="RUSclt_ru:Name300Type">
        <xs:annotation>
          <xs:documentation>Фамилия, имя для физических лиц/Наименование иного лица</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerMoneyAddress" type="RUScat_ru:RUAddressType">
        <xs:annotation>
          <xs:documentation>Адрес местожительства для физического лица/Адрес местонахождения (юридический адрес) для иных лиц</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="RUScat_ru:AmountType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сумма, номинальная стоимость денежных средств и (или) денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MonetaryInstrumentName" type="clt_ru:NameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование денежного инструмента</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Quantity" type="clt_ru:Quantity4Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество денежных инструментов</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PassDeclGoodsInfoType">
    <xs:annotation>
      <xs:documentation>Сведения о товарах</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TransferPurposeCode" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Кодовое обозначение цели ввоза, вывоза или нахождения товаров на таможенной территории Евразийского экономического союза                          1 – ввоз (свободное обращение);
3 – вывоз;
4 – временный вывоз;
5 – транзит
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AviaSign" type="clt_ru:IndicatorType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Признак ввоза товаров воздушным видом транспорта</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsCategoryCode" type="clt_ru:Code2Type" minOccurs="0" maxOccurs="10">
        <xs:annotation>
