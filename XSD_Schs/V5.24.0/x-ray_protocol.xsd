  </xs:annotation>
  <xs:element name="x-ray_protocol" type="xrp:x-ray_protocolType">
    <xs:annotation>
      <xs:documentation>Протокол анализа рентгеноскопического изображения</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="x-ray_protocolType">
    <xs:annotation>
      <xs:documentation>Протокол анализа рентгеноскопического изображения</xs:documentation>
    </xs:annotation>
