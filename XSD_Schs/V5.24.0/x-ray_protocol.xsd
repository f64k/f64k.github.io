        </xs:annotation>
      </xs:element>
      <xs:element name="value" type="clt_ru:FreeText500Type">
        <xs:annotation>
          <xs:documentation>Значение столбца таблицы</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="line" type="xrp:lineType" minOccurs="0" maxOccurs="unbounded">
