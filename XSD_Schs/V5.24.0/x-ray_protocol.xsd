        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="diagramma1Type">
    <xs:annotation>
      <xs:documentation>Свойства диаграммы загрузки / тождественности</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="clt_ru:Text150Type">
