        </xs:annotation>
      </xs:element>
      <xs:element name="top_name" type="clt_ru:Text50Type">
        <xs:annotation>
          <xs:documentation>Название отображаемое для верхней полоски диаграммы</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="top_color_font" type="clt_ru:Code6Type">
