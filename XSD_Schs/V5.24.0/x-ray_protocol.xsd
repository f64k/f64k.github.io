      <xs:element name="label_y" type="clt_ru:Count5Type">
        <xs:annotation>
          <xs:documentation>Координата Y расположения подписи для выделенного полигона области</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="label_color" type="clt_ru:Code6Type">
        <xs:annotation>
