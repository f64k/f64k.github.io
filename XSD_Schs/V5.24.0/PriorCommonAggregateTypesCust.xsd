<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.24.0" xmlns:RUSclt_ru="urn:customs.ru:RUSCommonLeafTypes:5.21.0" xmlns:RUScat_ru="urn:customs.ru:RUSCommonAggregateTypes:5.24.0" xmlns:RUDECLcat="urn:customs.ru:RUDeclCommonAggregateTypesCust:5.24.0" xmlns:cltTrans_ru="urn:customs.ru:Information:TransportDocuments:TransportCommonLeafTypesCust:5.14.3" xmlns:cltpi_ru="urn:customs.ru:Information:PriorInformation:PriorCommonLeafTypes:5.17.0" xmlns:catpi_ru="urn:customs.ru:Information:PriorInformation:PriorCommonAggregateTypes:5.24.0" targetNamespace="urn:customs.ru:Information:PriorInformation:PriorCommonAggregateTypes:5.24.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.24.0.0">
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.24.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:RUSCommonLeafTypes:5.21.0" schemaLocation="RUSCommonLeafTypes.xsd" />
  <xs:import namespace="urn:customs.ru:RUSCommonAggregateTypes:5.24.0" schemaLocation="RUSCommonAggregateTypes.xsd" />
  <xs:import namespace="urn:customs.ru:Information:TransportDocuments:TransportCommonLeafTypesCust:5.14.3" schemaLocation="TransportCommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:PriorInformation:PriorCommonLeafTypes:5.17.0" schemaLocation="PriorCommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:RUDeclCommonAggregateTypesCust:5.24.0" schemaLocation="RUDeclCommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Общие типы для документов предварительного информирования</xs:documentation>
  </xs:annotation>
  <xs:complexType name="AddressType">
    <xs:annotation>
      <xs:documentation>Адрес </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PostalCode" type="clt_ru:PostalCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Почтовый индекс.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountryCode" type="clt_ru:CountryA2CodeType">
        <xs:annotation>
          <xs:documentation>Буквенный код страны.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CounryName" type="clt_ru:CountryNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Краткое наименование страны.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Region" type="clt_ru:RegionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Область (регион, штат, провинция и т.п.)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="City" type="clt_ru:CityNameType">
        <xs:annotation>
          <xs:documentation>Населенный пункт.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StreetHouse" type="clt_ru:StreetHouseType">
        <xs:annotation>
          <xs:documentation>Улица, номер дома, номер офиса.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ControlledItemsDetailsType">
    <xs:annotation>
      <xs:documentation>Сведения о припасах, лекарственных средствах, опасных грузах, запасных частях и оборудовании, оружии и (или) боеприпасах, подлежащих указанию </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="InformationKindCode" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Код вида информации:                        1 - припасы;
2 - лекарственные средства, в составе которых содержатся наркотические, сильнодействующие средства, психотропные и ядовитые вещества;
3 - опасные товары (исключая оружие и (или) боеприпасы);
4 - запасные части и оборудование;
5 - оружие и (или) боеприпасы.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PresenceIndicator" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Признак наличия (0 - нет; 1 - есть)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ItemDetails" type="catpi_ru:ItemDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Наименование и количество</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ControlNoCustomsType">
    <xs:annotation>
      <xs:documentation>Сведения об иных видах контроля</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SanitaryQuarantine" type="catpi_ru:SanitaryQuarantineType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Cанитарно-карантинный контроль</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Veterinary" type="catpi_ru:VeterinaryType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Ветеринарный контроль</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FitoSanitary" type="catpi_ru:FitoSanitaryType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Фитосанитарный контроль</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CountryInformationType">
    <xs:annotation>
      <xs:documentation>Сведения о стране</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CountryName" type="clt_ru:CountryNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование страны</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountryACode" type="clt_ru:CountryA2CodeType">
        <xs:annotation>
          <xs:documentation>Буквенный код страны</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountryPlace" type="clt_ru:NameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Место отгрузки / место назначения</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CountryPortInfoType">
    <xs:annotation>
      <xs:documentation>Сведения о стране и порте</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CountryCode" type="clt_ru:CountryA2CodeType">
        <xs:annotation>
          <xs:documentation>Код страны</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PortName" type="clt_ru:NameType">
        <xs:annotation>
          <xs:documentation>Название порта</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CustomDocIDType">
    <xs:annotation>
      <xs:documentation>Регистрационный номер таможенного документа</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:GTDIDType">
        <xs:sequence>
          <xs:element name="Code" type="clt_ru:CodeVariable2Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Номер (идентификатор) вносимых изменений и (или) дополнений</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CustomsIdentificationMeansIdDetailsType">
    <xs:annotation>
      <xs:documentation>Сведения о средстве идентификации</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CustomsIdentificationMeansId" type="clt_ru:CodeVariable20Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Уникальный идентификатор (номер) средства идентификации</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DescriptionText" type="RUSclt_ru:FreeText4000Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Описание отличительных признаков</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ForeignCustomsIdentificationMeansIndicator" type="clt_ru:IndicatorType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Признак признания таможенным органом средств идентификации</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DestinationWarehouseType">
    <xs:annotation>
      <xs:documentation>СВХ места назначения</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CertificateNumber" type="clt_ru:DocumentNumberType">
        <xs:annotation>
          <xs:documentation>Номер Свидетельства о включении в Реестр / Номер лицензии </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CertificateDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Дата выдачи свидетельства о включении в Реестр владельцев СВХ / ТС / Дата выдачи лицензии </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DisinfectionDetailsType">
    <xs:annotation>
      <xs:documentation>Описание проведенного обеззараживания</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="EventDate" type="clt_ru:DateCustType">
        <xs:annotation>
          <xs:documentation>Дата проведения обеззараживания (дезинфекции)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExpositionDuration" type="RUSclt_ru:DurationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Продолжительность проведения обеззараживания (экспозиция)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DisinfectionMethodName" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Описание способа обработки</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChemicalName" type="clt_ru:FreeText250Type">
        <xs:annotation>
          <xs:documentation>Наименование химиката (действующего вещества)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TemperatureMeasure" type="RUSclt_ru:TemperatureMeasureType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Температура при проведении обеззараживания (дезинфекции)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ConcentrationMeasure" type="RUScat_ru:UnifiedPhysicalMeasureType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Концентрация химиката (действующего вещества), примененного при обеззараживании (дезинфекции)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DoseMeasure" type="RUScat_ru:UnifiedPhysicalMeasureType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Доза химиката (действующего вещества), примененного при обеззараживании (дезинфекции)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DescriptionText" type="RUSclt_ru:FreeText4000Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дополнительная информация</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DocBaseType">
    <xs:annotation>
      <xs:documentation>Компоненты, представляющие сведения о базовых реквизитах документа</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:DocumentBaseType">
        <xs:sequence>
          <xs:element name="DocStartDate" type="clt_ru:DateCustType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дата начала срока, в течение которого документ имеет силу</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DocValidityDate" type="clt_ru:DateCustType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дата окончания срока, в течение которого документ имеет силу</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код страны в соответствии с классификатором стран мира</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DocKindCode" type="clt_ru:CustomsDocumentCodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код вида документа в соответствии с классификатором видов документов и сведений, используемых при таможенном декларировании</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DocPermitType">
    <xs:annotation>
      <xs:documentation>Разрешительный документ</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:DocumentBaseType">
        <xs:sequence>
          <xs:element name="PresentedDocumentModeCode" type="clt_ru:CustomsDocumentCodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код вида документа в соответствии с классификатором, используемом для заполнения таможенных деклараций</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExpiredDate" type="clt_ru:DateCustType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Срок действия документа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NetWeightQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Вес нетто по документу</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GrossWeightQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Вес брутто по документу</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GoodsMeasureQuantity" type="catpi_ru:GoodsMeasureQuantityType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Количество товара по документу в дополнительных единицах измерения</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CodeFOIV" type="clt_ru:CodeVariable7Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код ФОИВ, выдавшего документ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FOIVName" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Наименование ФОИВ, выдавшего документ</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DriverInformationType">
    <xs:annotation>
      <xs:documentation>Информация о водителях</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:PersonBaseType">
        <xs:sequence>
          <xs:element name="IdentityCard" type="RUScat_ru:RUIdentityCardType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Документ, удостоверяющий личность водителя</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DTMDocDetailsType">
    <xs:annotation>
      <xs:documentation>Регистрационный номер декларации на транспортное средство, к которой был приложен представленный документ</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:GTDIDType">
        <xs:sequence>
          <xs:element name="UnifiedTransportModeCode" type="clt_ru:CodeVariable2Type">
            <xs:annotation>
              <xs:documentation>Код вида транспортного средства</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EECCustomsType">
    <xs:annotation>
      <xs:documentation>Таможенный орган ЕАЭС</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:CustomsType">
        <xs:sequence>
          <xs:element name="CountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код страны</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FitoSanitaryType">
    <xs:annotation>
      <xs:documentation>Фитосанитарный контроль</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FitoSanitaryDoc" type="catpi_ru:DocPermitType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Фитосанитарный сертификат страны-экспортера</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FitoCountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Буквенный код страны, выдавшей сертификат</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FitoCountryName" type="clt_ru:CountryNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование страны, выдавшей сертификат</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OriginPlace" type="clt_ru:NameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Место происхождения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DepartDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата отгрузки</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DisinfectionInfo" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Информация об обеззараживании </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Marking" type="clt_ru:FreeText250Type" minOccurs="0" maxOccurs="4">
        <xs:annotation>
          <xs:documentation>Сведения о маркировке</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GoodsDisinfectionType">
    <xs:annotation>
      <xs:documentation>Сведения об обеззараживании</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DisinfectionIndicator" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Обеззараживание проведено (0 - нет; 1 - да)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DisinfectionDetails" type="catpi_ru:DisinfectionDetailsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Описание проведенного обеззараживания</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GoodsMeasureQuantityType">
    <xs:annotation>
      <xs:documentation>Количество товара в дополнительных единицах измерения</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="GoodsQuantity" type="clt_ru:QuantityBasisType">
        <xs:annotation>
          <xs:documentation>Количество товара в единице измерения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MeasureUnitQualifierName" type="clt_ru:MeasureUnitQualifierNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Краткое наименование единицы измерения.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MeasureUnitQualifierCode" type="clt_ru:MeasureUnitQualifierCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код единицы измерения в соответствии с единицами измерения, применяемыми в Таможенном тарифе Российской Федерации.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GoodsPackagingType">
    <xs:annotation>
      <xs:documentation>Сведения о грузовых местах, и упаковке товаров</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PakageQuantity" type="clt_ru:PakageNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество грузовых мест.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CargoCode" type="clt_ru:PackageCodeType">
        <xs:annotation>
          <xs:documentation>Код вида груза в соответствии с  классификатором видов груза, упаковки и упаковочных материалов.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CargoDescription" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Описание вида груза.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Seal" type="cltTrans_ru:SealIDType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Номера пломб</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PackingInformation" type="catpi_ru:PackingInformationType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения об упаковке товаров. Код, Количество. Маркировка</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
