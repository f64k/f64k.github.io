<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:CustRep="urn:customs.ru:Information:CustomsDocuments:CustomsRepresentatives:5.24.0" xmlns:CategoryCust="urn:customs.ru:Categories:3.0.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.24.0" xmlns:RUScat_ru="urn:customs.ru:RUSCommonAggregateTypes:5.24.0" targetNamespace="urn:customs.ru:Information:CustomsDocuments:CustomsRepresentatives:5.24.0" elementFormDefault="qualified" version="5.24.0.0">
  <xs:import namespace="urn:customs.ru:Categories:3.0.0" schemaLocation="CategoriesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.24.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:RUSCommonAggregateTypes:5.24.0" schemaLocation="RUSCommonAggregateTypes.xsd" />
  <xs:annotation>
    <xs:documentation>Государственная услуга ведения реестра таможенных представителей</xs:documentation>
  </xs:annotation>
  <xs:element name="CustomsRepresentatives" type="CustRep:CustomsRepresentativesType">
    <xs:annotation>
      <xs:documentation>Государственная услуга ведения реестра таможенных представителей</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CustomsRepresentativesType">
    <xs:annotation>
      <xs:documentation>Государственная услуга ведения реестра таможенных представителей</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="ApplicationInfo" type="CustRep:ApplicationInfoType">
            <xs:annotation>
              <xs:documentation>Общая информация о заявлении</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Applicant" type="CustRep:ApplicantType">
            <xs:annotation>
              <xs:documentation>Общая информация о заявителе</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element name="RegistryInclusion" type="CustRep:RegistryInclusionType">
              <xs:annotation>
                <xs:documentation>Включение российского юридического лица в реестр таможенных представителей</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="RegistryPreliminaryDecision" type="CustRep:RegistryPreliminaryDecisionType">
              <xs:annotation>
                <xs:documentation>Принятие предварительного решения о соблюдении иных условий включения юридического лица в реестр таможенных представителей</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="RegistryAlteration" type="CustRep:RegistryAlterationType">
              <xs:annotation>
                <xs:documentation>Внесение изменений в реестр таможенных представителей</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="RegistryExclusion" type="CustRep:RegistryExclusionType">
              <xs:annotation>
                <xs:documentation>Исключение юридического лица из реестра таможенных представителей</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element name="AppliedDocuments" type="CustRep:AppliedDocumentsType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Блок прилагаемых документов</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentIDType" use="required" fixed="1006262E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные  типы</xs:documentation>
  </xs:annotation>
  <xs:simpleType name="String256Type">
    <xs:annotation>
      <xs:documentation>Текст от 1 до 256 символов</xs:documentation>
    </xs:annotation>
    <xs:restriction base="CategoryCust:CodeCategoriesCust">
      <xs:minLength value="1" />
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AccountsType">
    <xs:annotation>
      <xs:documentation>Сведения о банковских счетах</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RurAccount" type="CustRep:AccountType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения о банковских счетах в РФ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ForeignAccount" type="CustRep:AccountType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения о иностранных банковских счетах</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AccountType">
    <xs:annotation>
      <xs:documentation>Сведения об банковском счете</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BankInformationType">
        <xs:sequence>
          <xs:element name="OpenAccountDate" type="clt_ru:DateCustType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дата открытия счета</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BankINN" type="clt_ru:INNIDType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>ИНН - Индивидуальный номер налогоплательщика.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BankLegalLocation" type="RUScat_ru:RUAddressType">
            <xs:annotation>
              <xs:documentation>Юридический адрес банка</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BankRealLocation" type="RUScat_ru:RUAddressType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Адрес расположения банка</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ApplicantType">
    <xs:annotation>
      <xs:documentation>Данные о заявителе</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SigningDetails" type="CustRep:SigningDetailsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Физлицо (руководитель организации или уполномоченное лицо), подписавшее документ </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LegalEntity" type="CustRep:LegalEntityType">
        <xs:annotation>
          <xs:documentation>Заявитель</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationInfoType">
    <xs:annotation>
      <xs:documentation>Общая информация о заявлении</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ApplicantID" type="clt_ru:DocumentNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Уникальный идентификатор заявителя</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApplicationSourceSystemID" type="clt_ru:DocumentIDType">
        <xs:annotation>
          <xs:documentation>Номер заявления на ЕПГУ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApplicationSourceSystemDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата заявления на ЕПГУ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApplicationID" type="clt_ru:DocumentIDType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер заявления на АПС "Электронные Госуслуги-2"</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApplicationDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата регистрации заявления на АПС "Электронные Госуслуги-2"</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Department" type="cat_ru:CustomsType">
        <xs:annotation>
          <xs:documentation>Код подразделения, в которое предоставляется заявка</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AppliedDocumentsType">
    <xs:annotation>
      <xs:documentation>Блок прилагаемых документов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AppliedDocument" type="CustRep:AppliedDocumentType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Прилагаемый документ</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AppliedDocumentType">
    <xs:annotation>
      <xs:documentation>Прилагаемый документ</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Code" type="clt_ru:Code2Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код вида документа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Имя файла документа (без расширения)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Number" type="clt_ru:Quantity5Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер файла документа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Extension" type="clt_ru:CodeVariable4Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Расширение файла</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Base" type="clt_ru:Text3MB" minOccurs="0" maxOccurs="2">
        <xs:annotation>
          <xs:documentation>Файл в формате base64</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Description" type="clt_ru:FreeText500Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Комментарий</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BankGuaranteeInfoType">
    <xs:annotation>
      <xs:documentation>Реквизиты банковской гарантии</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DocDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата выдачи банковской гарантии</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DocNumber" type="clt_ru:DocumentNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер банковской гарантии</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CashDepositInfoType">
    <xs:annotation>
      <xs:documentation>Реквизиты денежного залога</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DocDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата платежного поручения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DocNumber" type="clt_ru:CodeVariable6Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChangedApplicantInfo">
    <xs:annotation>
      <xs:documentation>Сведения об изменении информации о заявителе (для услуг ведения реестров)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ApplicantInfo" type="CustRep:ApplicantType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Изменения в информации о заявителе</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ChangeInfoType">
    <xs:annotation>
      <xs:documentation>Сведения, подлежащие изменению</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CustRep:ChangedApplicantInfo">
        <xs:sequence>
          <xs:element name="InformationAboutSecuring" type="CustRep:InformationAboutSecuringType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения об обеспечении уплаты таможенных пошлин, налогов</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RiskInsurance" type="CustRep:RiskInsuranceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о договоре страхования риска гражданской ответственности</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Specialists" type="CustRep:SpecialistsType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о специалистах по таможенным операциям</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LimitsData" type="CustRep:LimitsDataType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения об ограничениях деятельности в качестве таможенного представителя</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ContractOfGuaranteeInfoType">
    <xs:annotation>
      <xs:documentation>Реквизиты договора поручительства</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DocDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата заключения договора поручительства</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DocNumber" type="CustRep:String256Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер договора поручительства</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CustomsReceiptRegNumberType">
    <xs:annotation>
      <xs:documentation>Регистрационный номер таможенной расписки</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:GTDIDType">
        <xs:sequence>
          <xs:element name="DocNumberPrefix" type="clt_ru:Code2Type">
            <xs:annotation>
              <xs:documentation>Префикс порядкового номера таможенной расписки</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DirectorsType">
    <xs:annotation>
      <xs:documentation>Сведения о членах совета директоров (наблюдательного совета), членах коллегиального исполнительного органа</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Director" type="cat_ru:PersonBaseType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Член совета директоров (наблюдательного совета)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Collegial" type="cat_ru:PersonBaseType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Член коллегиального исполнительного органа</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EmployeesType">
    <xs:annotation>
      <xs:documentation>Сведения о работниках, в чьи должностные обязанности входят организация таможенных операций и (или) их осуществление</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Employee" type="CustRep:EmployeeType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Работник</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EmployeeType">
    <xs:annotation>
      <xs:documentation>Работник</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CustRep:PersonDutiesType">
        <xs:sequence>
          <xs:element name="InfoAboutEmployees" type="clt_ru:FreeText500Type" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Дополнительные сведения о работнике</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FilialsType">
    <xs:annotation>
      <xs:documentation>Обособленные структурные подразделения</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Filial" type="CustRep:FilialType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Информация о подразделениях</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FilialType">
    <xs:annotation>
      <xs:documentation>Обособленное структурное подразделение</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="RUScat_ru:SubjectBaseDetailsType">
        <xs:sequence>
          <xs:element name="FilialLocation" type="RUScat_ru:RUAddressType">
            <xs:annotation>
              <xs:documentation>Место нахождения структурного подразделения</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FilialManager" type="CustRep:OrganisationHeadType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о руководителе структурного подразделения</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RegularStaffQuantity" type="clt_ru:Quantity5Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о штатной численности подразделения</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Communications" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о контактной информации филиала</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Specialists" type="CustRep:SpecialistsType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о специалистах по таможенным операциям в обособленных подразделениях
							</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FNSInfoType">
    <xs:annotation>
      <xs:documentation>Свидетельство о постановке на налоговый учет</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FNSDocumentSeries" type="clt_ru:CodeVariable10Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Серия свидетельства о постановке на налоговый учет</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FNSDocumentNumber" type="clt_ru:CodeVariable20Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер свидетельства о постановке на налоговый учет</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FNSDocumentDate" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата выдачи свидетельства о постановке на налоговый учет</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IFNSInfo" type="CustRep:IFNSType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Информация об ИФНС выдавшем свидетельство</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FoundersType">
    <xs:annotation>
      <xs:documentation>Сведения об учредителях и (или) участниках заявителя и о доле их участия в уставном (складском) капитале заявителя</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Founder" type="CustRep:FounderType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Сведения об учредителе (участнике) и о доле его участия в уставном (складском) капитале</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Note" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дополнительная информация об учредителях (участниках)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FounderType">
    <xs:annotation>
      <xs:documentation>Учредитель и (или) участник</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ParticipationType" type="clt_ru:Code2Type">
        <xs:annotation>
          <xs:documentation>01 - Учредитель, 02 - Участник</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="PersonFounder" type="CustRep:PhysicalPersonType">
          <xs:annotation>
            <xs:documentation>Учредитель (участник) - физлицо</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="LegalEntityFounder" type="RUScat_ru:SubjectBaseDetailsType">
          <xs:annotation>
            <xs:documentation>Учредитель (участник) - юрлицо</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="Share" type="clt_ru:FreeText500Type">
        <xs:annotation>
          <xs:documentation>Доля участия в уставном (складском) капитале</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FullContactInfoType">
    <xs:annotation>
      <xs:documentation>Полная контактная информация</xs:documentation>
    </xs:annotation>
