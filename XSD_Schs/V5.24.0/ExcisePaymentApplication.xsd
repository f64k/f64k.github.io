              <xs:documentation>Сведения о производителе товаров</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AppBuyExciseStampsDocument" type="cat_ru:DocumentBaseType" minOccurs="1" maxOccurs="unbounded">
